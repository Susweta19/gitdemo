                              <Alignment>
                                <Actions>
                                  <SelectByText>
                                    <Parameters>
                                      <text>Baseline</text>
                                    </Parameters>
                                  </SelectByText>
                                </Actions>
                              </Alignment>
                              <Insert />
                            </Actions>
                          </Content>
                        </Actions>
                      </InsertImage>
                    </Actions>
