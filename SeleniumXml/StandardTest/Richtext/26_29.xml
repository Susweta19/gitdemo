                                  <path>#DocPath#Picture5.jpg</path>
                                </Parameters>
                              </SelectFile>
                              <ChangeTab>
                                <Parameters>
                                  <tabName>Appearance</tabName>
                                </Parameters>
                              </ChangeTab>
