<?xml version="1.0" encoding="utf-8"?>
<!--"Check the ability to align the selected Image
'Alignment'' dropdown list"-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the ability to align the selected Image 'Alignment'' dropdown list</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_for_Richtext</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <RichText>
                    <Parameters>
                      <attrId>92</attrId>
                    </Parameters>
                    <Actions>
                      <Clear />
                      <InsertImage>
                        <Actions>
                          <Content>
                            <Actions>
                              <SelectFile>
                                <Parameters>
                                  <path>#DocPath#Picture5.jpg</path>
                                </Parameters>
                              </SelectFile>
                              <ChangeTab>
                                <Parameters>
                                  <tabName>Appearance</tabName>
                                </Parameters>
                              </ChangeTab>
                              <!--Select align for picture-->
                              <Alignment>
                                <Actions>
                                  <SelectByText>
                                    <Parameters>
                                      <text>Up</text>
                                    </Parameters>
                                  </SelectByText>
                                </Actions>
                              </Alignment>
                              <Insert />
                            </Actions>
                          </Content>
                        </Actions>
                      </InsertImage>
                    </Actions>
                  </RichText>
                </Actions>
              </Content>
              <Sleep>
                <Parameters>
                  <timeOut>2</timeOut>
                </Parameters>
              </Sleep>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <ReadForm>
            <Actions>
              <Sleep>
                <Parameters>
                  <timeOut>2</timeOut>
                </Parameters>
              </Sleep>
              <RichText>
                <Parameters>
                  <attrId>92</attrId>
                </Parameters>
                <Actions>
                  <!--Check Align for picutre-->
                  <ReadAlignment>
                    <Parameters>
                      <expectedAlignment>top</expectedAlignment>
                    </Parameters>
                  </ReadAlignment>
                </Actions>
              </RichText>
            </Actions>
          </ReadForm>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <RichText>
                    <Parameters>
                      <attrId>92</attrId>
                    </Parameters>
                    <Actions>
                      <Clear />
