<?xml version="1.0" encoding="utf-8"?>
<!--Check the availability of the 'Title' textbox-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the availability of the 'Insert/edit image' window --&gt;  'General' Tab --&gt; Check the availability of the 'Picture description' textbox</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_for_Richtext</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <RichText>
                    <Parameters>
                      <attrId>92</attrId>
                    </Parameters>
                    <Actions>
                      <SelectAll></SelectAll>
                      <Clear></Clear>
                      <InsertImage>
                        <Actions>
                          <Content>
                            <Actions>
                              <SelectFile>
                                <Parameters>
                                  <path>#DocPath#Picture4.jpg</path>
                                </Parameters>
                              </SelectFile>
                              <WritePictureDetails>
                                <Parameters>
                                  <Description>Nice</Description>
                                  <Title>Image</Title>
                                </Parameters>
                              </WritePictureDetails>
                              <Insert />
                            </Actions>
                          </Content>
                        </Actions>
                      </InsertImage>
                    </Actions>
                  </RichText>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <ReadForm>
            <Actions>
              <Sleep>
                <Parameters>
                  <timeOut>3</timeOut>
                </Parameters>
              </Sleep>
              <RichText>
                <Parameters>
                  <attrId>92</attrId>
                </Parameters>
                <Actions>
                  <ReadPictureDescription>
                    <ExpectedResult>
                      <value>Nice</value>
                    </ExpectedResult>
                  </ReadPictureDescription>
                  <ReadPictureTitle>
                    <ExpectedResult>
                      <value>Image</value>
                    </ExpectedResult>
                  </ReadPictureTitle>
                </Actions>
              </RichText>
            </Actions>
          </ReadForm>
          <Logout>
          	<Actions>
          		<ReadPopUpMessage>
          			<Actions>
          				<ElementAt>
          					<Parameters>
          						<index>0</index>
          					</Parameters>
          					<ExpectedResult>Do you really want to quit TEEXMA?</ExpectedResult>
          				</ElementAt></Actions>
          		</ReadPopUpMessage>
          		<CloseConfirmationPopup>
          			<Parameters>
          				<result>true</result>
          			</Parameters>
          		</CloseConfirmationPopup></Actions>
          </Logout>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>