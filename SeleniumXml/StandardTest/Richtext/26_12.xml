<?xml version="1.0" encoding="utf-8"?>
<!--Try to type in several words, click on the 'Select Text Color' dropdown 
	table and select a color -->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the availability of the 'Select Text Color' button and the dropdown table</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Message>Associativeteststart</Message>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_for_Richtext</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>New</path>
											</nodes>
										</Parameters>
									</SelectEntity>
								</Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<Edit>
						<Actions>
							<Content>
								<Actions>
									<RichText>
										<Parameters>
											<attrId>92</attrId>
										</Parameters>
										<Actions>
											<Clear />
											<SetFontcolor>
												<Parameters>
													<ColorTitle>Blue</ColorTitle>
												</Parameters>
												<Actions>
												</Actions>
											</SetFontcolor>
											<Write>
												<Parameters>
													<value>Rest</value>
												</Parameters>
											</Write>
										</Actions>
									</RichText>
								</Actions>
							</Content>
							<Ok>
								<Parameters>
									<validate>true</validate>
								</Parameters>
							</Ok>
						</Actions>
					</Edit>
					<ReadForm>
						<Actions>
							<RichText>
								<Parameters>
									<attrId>92</attrId>
								</Parameters>
								<Actions>
									<ReadTextColor>
										<Parameters>
											<ExpectedTextColor>Blue</ExpectedTextColor>
										</Parameters>
									</ReadTextColor>
								</Actions>
							</RichText>
						</Actions>
					</ReadForm>
					<Edit>
						<Actions>
							<Content>
								<Actions>
									<RichText>
										<Parameters>
											<attrId>92</attrId>
										</Parameters>
										<Actions>
											<Write>
												<Parameters>
													<value>Susweta </value>
												</Parameters>
											</Write>
											<SelectAll></SelectAll>
											<SetFontcolor>
												<Parameters>
													<ColorTitle>Red</ColorTitle>
												</Parameters>
												<Actions>
												</Actions>
											</SetFontcolor>
										</Actions>
									</RichText>
								</Actions>
							</Content>
							<Ok>
								<Parameters>
									<validate>true</validate>
								</Parameters>
							</Ok>
						</Actions>
					</Edit>
					<ReadForm>
						<Actions>
							<RichText>
								<Parameters>
									<attrId>92</attrId>
								</Parameters>
								<Actions>
									<ReadTextColor>
										<Parameters>
											<ExpectedTextColor>Red</ExpectedTextColor>
										</Parameters>
									</ReadTextColor>
								</Actions>
							</RichText>
						</Actions>
					</ReadForm>
					<Edit>
						<Actions>
							<Content>
								<Actions>
									<RichText>
										<Parameters>
											<attrId>92</attrId>
										</Parameters>
										<Actions>
											<Clear />
											<Write>
												<Parameters>
													<value>Red Blue</value>
												</Parameters>
											</Write>
											<PressKeyFromKeyBoard>
												<Actions>
													<PressHomeKey>
													</PressHomeKey>
													<PressRightArrow>
														<Parameters>
															<times>1</times>
														</Parameters>
													</PressRightArrow>
												</Actions>
											</PressKeyFromKeyBoard>

											<SetFontcolor>
												<Parameters>
													<ColorTitle>Red</ColorTitle>
												</Parameters>
												<Actions>
												</Actions>
											</SetFontcolor>
											<PressKeyFromKeyBoard>
												<Actions>
													<PressRightArrow>
														<Parameters>
															<times>5</times>
														</Parameters>
													</PressRightArrow>
												</Actions>
											</PressKeyFromKeyBoard>

											<SetFontcolor>
												<Parameters>
													<ColorTitle>Blue</ColorTitle>
												</Parameters>
												<Actions>
												</Actions>
											</SetFontcolor>
										</Actions>
									</RichText>
								</Actions>
							</Content>
							<Ok>
								<Parameters>
									<validate>true</validate>
								</Parameters>
							</Ok>
						</Actions>
					</Edit>
					<ReadForm>
						<Actions>
							<RichText>
								<Parameters>
									<attrId>92</attrId>
								</Parameters>
								<Actions>
									<ReadTextColor>
										<Parameters>
											<ExpectedTextColor>Red</ExpectedTextColor>
										</Parameters>
									</ReadTextColor>
									<ReadTextColor>
										<Parameters>
											<ExpectedTextColor>Blue</ExpectedTextColor>
										</Parameters>
									</ReadTextColor>
								</Actions>
							</RichText>
						</Actions>
					</ReadForm>
					<Logout>
          	<Actions>
          		<ReadPopUpMessage>
          			<Actions>
          				<ElementAt>
          					<Parameters>
          						<index>0</index>
          					</Parameters>
          					<ExpectedResult>Do you really want to quit TEEXMA?</ExpectedResult>
          				</ElementAt></Actions>
          		</ReadPopUpMessage>
          		<CloseConfirmationPopup>
          			<Parameters>
          				<result>true</result>
          			</Parameters>
          		</CloseConfirmationPopup></Actions>
          </Logout>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>