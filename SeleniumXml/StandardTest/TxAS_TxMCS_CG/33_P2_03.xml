<?xml version="1.0" encoding="utf-8"?>
<!--the script is running with breakpoint -->
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the functionality of  'Blank Requirements' button</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<IfPopUpPresent></IfPopUpPresent>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_CG+AS</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
						</Actions>
					</NavigationTree>
					<LaunchModel>
						<Parameters>
							<modelName>TxMCS</modelName>
						</Parameters>
					</LaunchModel>
					<TxMCS>
						<Actions>
							<Content>
								<Actions>
									<BlankRequirementSet>
										<Parameters>
											<objectType>TEST_CG+AS</objectType>
										</Parameters>
									</BlankRequirementSet>
									<Settings>
										<Actions>
											<SelectEntity>
												<Parameters>
													<nodes>
														<path>TEST_CG+AS</path>
														<path>Bool/Date/Str/Text</path>
														<path>Bool/Date/Str/Text</path>
														<path>Boolean</path>
													</nodes>
												</Parameters>
											</SelectEntity>
										</Actions>
									</Settings>
									<Criteria>
										<Actions>
											<BooleanCriterion>
												<Parameters>
													<value>true</value>
												</Parameters>
											</BooleanCriterion>
										</Actions>
									</Criteria>
									<Next>
										<Parameters>
											<disable>false</disable>
										</Parameters>
									</Next>
									<Sleep>
										<Parameters>
											<timeOut>2</timeOut>
										</Parameters>
									</Sleep>
									<SelectionResult>
										<Actions>

											<EntitytPresence>
												<Parameters>
													<nodes>
														<path>BooleanTrue</path>
													</nodes>
													<presence>true</presence>
												</Parameters>
											</EntitytPresence>
										</Actions>

									</SelectionResult>
									<ExportEntitiesToExcel />
									<Sleep>
										<Parameters>
											<timeOut>15</timeOut>
										</Parameters>
									</Sleep>
								</Actions>
							</Content>
						</Actions>
					</TxMCS>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>