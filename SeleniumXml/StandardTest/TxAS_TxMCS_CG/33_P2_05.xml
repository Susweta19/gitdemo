<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Relation between the attribute checkbox(Left side) and the define criteria(Right side)---&gt;Try to check the checkbox </testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<IfPopUpPresent></IfPopUpPresent><NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_CG+AS</value>
										</Parameters>
									</Select></Actions>
							</SelectOT></Actions>
					</NavigationTree>
					<LaunchModel>
						<Parameters>
							<modelName>TxMCS</modelName>
						</Parameters>
					</LaunchModel>
					<TxMCS>
						<Actions>
							<Content>
								<Actions>
									<BlankRequirementSet>
										<Parameters>
											<objectType>TEST_CG+AS</objectType>
										</Parameters>
									</BlankRequirementSet>
									<Settings>
										<Actions>
											<SelectEntityBox>
												<Parameters><nodes><path>TEST_CG+AS</path><path>Bool/Date/Str/Text</path><path>Bool/Date/Str/Text</path><path>Date</path></nodes></Parameters>
												<Actions>
													<Tick />
													<Read>
														<ExpectedResult>
																true
															</ExpectedResult>
													</Read></Actions>
											</SelectEntityBox><!--We did not define criteria so it is unticked-->
											<SelectEntityBox>
												<Parameters><nodes><path>TEST_CG+AS</path><path>Bool/Date/Str/Text</path><path>Bool/Date/Str/Text</path><path>Boolean</path></nodes></Parameters>
												<Actions>
													<Read>
														<ExpectedResult>false</ExpectedResult>
													</Read></Actions>
											</SelectEntityBox>
											<SelectEntity>
												<Parameters><nodes><path>TEST_CG+AS</path><path>Bool/Date/Str/Text</path><path>Bool/Date/Str/Text</path><path>Boolean</path></nodes></Parameters>
											</SelectEntity></Actions>
									</Settings><!--we define criteria-->
									<Criteria>
										<Actions>
											
											<BooleanCriterion>
												<Parameters>
													<value>true</value>
												</Parameters>
											</BooleanCriterion></Actions>
									</Criteria><!--Now it is ticked automatically-->
									<Settings>
										<Actions>
											<SelectEntityBox>
												<Parameters><nodes><path>TEST_CG+AS</path><path>Bool/Date/Str/Text</path><path>Bool/Date/Str/Text</path><path>Boolean</path></nodes></Parameters>
												<Actions>
													<Read>
														<ExpectedResult>
																true
															</ExpectedResult>
													</Read></Actions>
											</SelectEntityBox></Actions>
									</Settings>
								</Actions>
							</Content></Actions>
					</TxMCS></Actions>
			</MainWindow></Actions></TxInterface></Actions>