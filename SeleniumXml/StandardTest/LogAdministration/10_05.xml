<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>"Check the 'Selection' action when a Multicriteria Selection is launched
(out of Widget)"</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_LogAdmin</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
            </Actions>
          </NavigationTree>
          <OpenRequirementSet>
            <Actions>
              <Content>
                <Actions>
                  <!--weopenarequirementset-->
                  <RequirementSet>
                    <Parameters>
                      <requirementSet>Date</requirementSet>
                    </Parameters>
                  </RequirementSet>
                  <Open>
                    <Parameters>
                      <popUp>false</popUp>
                    </Parameters>
                    <Actions>
                      <Close>
                      </Close>
                    </Actions>
                  </Open>
                </Actions>
              </Content>
            </Actions>
          </OpenRequirementSet>
          <OpenRequirementSet>
            <Actions>
              <Content>
                <Actions>
                  <!--wemodifyarequirementset-->
                  <RequirementSet>
                    <Parameters>
                      <requirementSet>Modify</requirementSet>
                    </Parameters>
                  </RequirementSet>
                  <Open>
                    <Parameters>
                      <popUp>false</popUp>
                    </Parameters>
                    <Actions>
                      <Content>
                        <Actions>
                          <CriteriaTab>
                            <Actions>
                              <Content>
                                <Actions>
                                  <DefineCriteria>
                                    <Actions>
                                      <Content>
                                        <Actions>
                                          <ListOfAttributes>
                                            <Actions>
                                              <SelectEntity>
                                                <Parameters>
                                                  <nodes>
                                                    <path>TEST_LogAdmin</path>
                                                    <path>Bool/Date/Str/Text</path>
                                                    <path>Boolean</path>
                                                  </nodes>
                                                </Parameters>
                                              </SelectEntity>
                                            </Actions>
                                          </ListOfAttributes>
                                          <BooleanCriterion>
                                            <Parameters>
                                              <value>
																								true
																							</value>
                                            </Parameters>
                                          </BooleanCriterion>
                                        </Actions>
                                      </Content>
                                      <Ok>
                                        <Parameters>
                                          <validate>
																						true
																					</validate>
                                        </Parameters>
                                      </Ok>
                                    </Actions>
                                  </DefineCriteria>
                                </Actions>
                              </Content>
                            </Actions>
                          </CriteriaTab>
                          <Sleep>
                            <Parameters>
                              <timeOut>2</timeOut>
                            </Parameters>
                          </Sleep>
                          <CriteriaTab>
                            <Actions>
                              <Content>
                                <Actions>
                                  <Save>
                                    <Actions>
                                    	<ClosePopupWithinFrame>
                                    		<Parameters>
                                    			<result>true</result>
                                    		</Parameters>
                                    	</ClosePopupWithinFrame>
                                    	
                                    </Actions>
                                  </Save>
                                </Actions>
                              </Content>
                            </Actions>
                          </CriteriaTab>
                        </Actions>
                      </Content>
                      <Close>
                      </Close>
                    </Actions>
                  </Open>
                </Actions>
              </Content>
            </Actions>
          </OpenRequirementSet>
          <OpenRequirementSet>
            <Actions>
              <Content>
                <Actions>
                  <!--welauncharequirementset-->
                  <RequirementSet>
                    <Parameters>
                      <requirementSet>Launch</requirementSet>
                    </Parameters>
                  </RequirementSet>
                  <Open>
                    <Parameters>
                      <popUp>false</popUp>
                    </Parameters>
                    <Actions>
                      <Content>
                        <Actions>
                          <CriteriaTab>
                            <Actions>
                              <Content>
                                <Actions>
                                  <LaunchCalculation>
                                    <Actions>
                                    </Actions>
                                  </LaunchCalculation>
                                </Actions>
                              </Content>
                            </Actions>
                          </CriteriaTab>
                        </Actions>
                      </Content>
                    </Actions>
                  </Open>
                </Actions>
              </Content>
            </Actions>
          </OpenRequirementSet>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>