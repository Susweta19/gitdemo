<?xml version="1.0" encoding="utf-8"?>
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the Attribute data placement for XMLTemplate file Multiple Associative </testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_Export</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>TestIndia</path>
											</nodes>
										</Parameters>
									</SelectEntity>
								</Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<Export>
						<Actions>
							<Content>
								<Actions>
									<Exports>
										<Actions>
											<Select>
												<Parameters>
													<value>[XML]&#160;Standard&#160;Export&#160;[India]</value>
												</Parameters>
											</Select>
										</Actions>
									</Exports>
									<DataTree>
										<Actions>
											<UncheckAll></UncheckAll>
											<SelectEntitiesBox>
												<Parameters>
													<collection>
														<collection>
															<path>Associative</path>
															<path>Multiple Associative</path>
														</collection>
													</collection>
												</Parameters>
												<Actions>
													<Tick></Tick>
												</Actions>
											</SelectEntitiesBox>

										</Actions>
									</DataTree>
									<Export></Export>
								</Actions>
							</Content>
						</Actions>
					</Export>
					<Sleep>
						<Parameters>
							<timeOut>2</timeOut>
						</Parameters>
					</Sleep>
					<ManageDownloadedFiles>
						<Actions>
							<GetLastDownloadedFile>
								<Parameters>
									<DownloadDirectory>#DownloadDirectory#</DownloadDirectory>
								</Parameters>
								<Hashtag>#downloadDir#</Hashtag>
							</GetLastDownloadedFile>
							<ReadXmlAttrValueByName>
								<Parameters>
									<filePath>#downloadDir#</filePath>
									<nodePath>/ObjectsAndData</nodePath>
									<attributeName>sUser</attributeName>
								</Parameters>
								<ExpectedResult>Administrator</ExpectedResult>
							</ReadXmlAttrValueByName>
							<ReadXmlAttrValueByName>
								<Parameters>
									<filePath>#downloadDir#</filePath>
									<nodePath>.//ObjectsAndData/OT</nodePath>
									<attributeName>sName</attributeName>
								</Parameters>
								<ExpectedResult>TEST_Export</ExpectedResult>
							</ReadXmlAttrValueByName>
							<ReadXmlAttrValueByName>
								<Parameters>
									<filePath>#downloadDir#</filePath>
									<nodePath>.//Objs/DataObj</nodePath>
									<attributeName>TestIndia</attributeName>
								</Parameters>
							</ReadXmlAttrValueByName>
							<ReadXmlAttrValueByName>
								<Parameters>
									<filePath>#downloadDir#</filePath>
									<nodePath>.//Data/DLnk/AssObj[1]</nodePath>
									<attributeName>sName</attributeName>
								</Parameters>
								<ExpectedResult>New - New</ExpectedResult>
							</ReadXmlAttrValueByName>
							<ReadXmlAttrValueByName>
								<Parameters>
									<filePath>#downloadDir#</filePath>
									<nodePath>.//Data/DLnk/AssObj[2]</nodePath>
									<attributeName>sName</attributeName>
								</Parameters>
								<ExpectedResult>New - New</ExpectedResult>
							</ReadXmlAttrValueByName>
							<!-- The below attributeName is not present in the tag LLAE430_SecuredArchivedFiles, 
								need to check for Standard Version -->
							<!-- <ReadXmlAttrValueByName> <Parameters> <filePath>#downloadDir#</filePath> 
								<nodePath>.//Data/DLnk/AssObj[3]</nodePath> <attributeName>sName</attributeName> 
								</Parameters> <ExpectedResult>New - New</ExpectedResult> </ReadXmlAttrValueByName> -->
						</Actions>
					</ManageDownloadedFiles>

				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>