<?xml version="1.0" encoding="utf-8"?>
<!--Check the ability to open the 'Export' window-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the availability of the 'Compress the data file' button</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Export</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
              	<Actions>
              		<SelectEntity>
              			<Parameters>
              				<nodes>
              					<path>TestIndia</path></nodes>
              			</Parameters>
              		</SelectEntity>
              	</Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Export>
            <Actions>
              <ReadHeader>
              	<ExpectedResult>Export</ExpectedResult></ReadHeader>
              <Content>
              	<Actions>              		
              		<Exports>
              			<Actions>
              				<Select>
              					<Parameters>
              						<value>[Excel]&#160;Standard&#160;Export</value>
              					</Parameters>
              				</Select></Actions>
              		</Exports>
              		<WaitForAjax></WaitForAjax>
              		<DataTree>
              			<Actions>
              				<SelectEntitiesBox>
              					<Parameters>
              						<collection>
              							<collection>
              								<path>Documents</path>
              								<path>Document_File</path></collection>
              							<collection>
              							<path>Documents</path>
              								<path>{Document_File}</path></collection></collection>
              					</Parameters>
              					<Actions>
              						<Tick></Tick></Actions>
              				</SelectEntitiesBox>
              			</Actions>
              		</DataTree>
              		<CompressFile>
              			<Actions>
              				<Tick></Tick></Actions>
              		</CompressFile>
              		<Export></Export>
              		<Sleep>
              			<Parameters>
              				<timeOut>15</timeOut>
              			</Parameters>
              		</Sleep></Actions>
              </Content>
              <Close></Close>
            </Actions>
          </Export>
          <DownloadFileChecking>
          	<Parameters>
          		<DownloadDirectory>#DownloadDirectory#</DownloadDirectory>
          		<fileName>#YYYY#-#MM#-#DD#_</fileName>
          		<contains>true</contains>
          	</Parameters>
          	<ExpectedResult>true</ExpectedResult>
          </DownloadFileChecking>
          <DownloadFileChecking>
          	<Parameters>
          		<DownloadDirectory>#DownloadDirectory#</DownloadDirectory>
          		<fileName>_Export.xlsm</fileName>
          		<contains>true</contains>
          	</Parameters>
          	<ExpectedResult>true</ExpectedResult>
          </DownloadFileChecking>
          
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>