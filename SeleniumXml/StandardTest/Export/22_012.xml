<?xml version="1.0" encoding="utf-8"?>
<!--Check the ability to open the 'Export' window-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the availability of the Data List</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Export</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
            </Actions>
          </NavigationTree>
          <Export>
            <Actions>
              <Content>
                <Actions>
                	<DataTree>
                		<Actions>
                			<IsButtonPresentByTitle>
                				<Parameters>
                					<buttonName>View the checked Attributes</buttonName>
                				</Parameters>
                				<ExpectedResult>true</ExpectedResult>
                			</IsButtonPresentByTitle>
                			<IsButtonPresentByTitle>
                				<Parameters>
                					<buttonName>Deselect all Attributes</buttonName>
                				</Parameters>
                				<ExpectedResult>true</ExpectedResult>
                			</IsButtonPresentByTitle>
                			<IsButtonPresentByTitle>
                				<Parameters>
                					<buttonName>Select all Attributes</buttonName>
                				</Parameters>
                				<ExpectedResult>true</ExpectedResult>
                			</IsButtonPresentByTitle>
                			<Search>
                				<Parameters>
                					<research>Bool</research>
                				</Parameters>
                			</Search>
                			<IsButtonPresentByTitle>
                				<Parameters>
                					<buttonName>Display the entire tree</buttonName>
                				</Parameters>
                				<ExpectedResult>true</ExpectedResult>
                			</IsButtonPresentByTitle></Actions>
                	</DataTree>
                </Actions>
              </Content>
              <Close></Close>
            </Actions>
          </Export>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>