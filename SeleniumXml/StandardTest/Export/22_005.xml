<?xml version="1.0" encoding="utf-8"?>
<!--Check the ability to open the 'Export' window-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the availability of the Objects List on change in Filter </testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Export</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
            </Actions>
          </NavigationTree>
          <Export>
            <Actions>
              <Content>
                <Actions>
                	<ObjectsTree>
                		<Actions>
                			<EntitytPresence>
                				<Parameters>
                					<nodes>
                						<path>TestIndia</path></nodes>
                					<presence>true</presence>
                				</Parameters>
                			</EntitytPresence>
                			<EntitytPresence>
                				<Parameters>
                					<nodes>
                						<path>Validation in progress</path></nodes>
                					<presence>false</presence>
                				</Parameters>
                			</EntitytPresence></Actions>
                	</ObjectsTree>
                	<Filtre>
                		<Actions>
                			<Select>
                				<Parameters>
                					<value>Notification&#160;type</value>
                				</Parameters>
                			</Select></Actions>
                	</Filtre>
                	<ObjectsTree>
                		<Actions>
                			<EntitytPresence>
                				<Parameters>
                					<nodes>
                						<path>Validation in progress</path></nodes>
                					<presence>true</presence>
                				</Parameters>
                			</EntitytPresence>
                			<EntitytPresence>
                				<Parameters>
                					<nodes>
                						<path>Validation cancelled</path></nodes>
                					<presence>true</presence>
                				</Parameters>
                			</EntitytPresence>
                			<EntitytPresence>
                				<Parameters>
                					<nodes>
                						<path>TestIndia</path></nodes>
                					<presence>false</presence>
                				</Parameters>
                			</EntitytPresence></Actions>
                	</ObjectsTree>
                	<Filtre>
                		<Actions>
                			<Select>
                				<Parameters>
                					<value>TEST_Export_Inheritance</value>
                				</Parameters>
                			</Select></Actions>
                	</Filtre>
                	<ObjectsTree>
                		<Actions>
                			<EntitytPresence>
                				<Parameters>
                					<nodes>
                						<path>T_R_I_1</path></nodes>
                					<presence>true</presence>
                				</Parameters>
                			</EntitytPresence>
                			<EntitytPresence>
                				<Parameters>
                					<nodes>
                						<path>T_R_I_2</path></nodes>
                					<presence>true</presence>
                				</Parameters>
                			</EntitytPresence>
                			<EntitytPresence>
                				<Parameters>
                					<nodes>
                						<path>TestIndia</path></nodes>
                					<presence>false</presence>
                				</Parameters>
                			</EntitytPresence>
                			</Actions>
                	</ObjectsTree>
                </Actions>
              </Content>
              <Close></Close>
            </Actions>
          </Export>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>