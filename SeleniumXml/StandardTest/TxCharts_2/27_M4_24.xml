<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Do the settings of the Exponential curve </testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<CurveModule>
						<Actions>
							<Content>
								<Actions>
									<AddNewCurve>
										<Actions>
											<Content>
												<Actions>
													<SourceType>
														<Parameters>
															<typeName>Logarithm function</typeName>
														</Parameters>
													</SourceType>
													<FunctionEquation>
														<Actions>
															<Read>
																<ExpectedResult>y = A + B * log(x)</ExpectedResult>
															</Read>
														</Actions>
													</FunctionEquation>
													<SetA>
														<Actions>
															<Write>
																<Parameters>
																	<value>1</value>
																</Parameters>
															</Write>
														</Actions>
													</SetA>
													<SetB>
														<Actions>
															<Write>
																<Parameters>
																	<value>1</value>
																</Parameters>
															</Write>
														</Actions>
													</SetB>
													<AbscissasLowerbound>
														<Actions>
															<Write>
																<Parameters>
																	<value>1</value>
																</Parameters>
															</Write>
														</Actions>
													</AbscissasLowerbound>
													<AbscissasUpperbound>
														<Actions>
															<Write>
																<Parameters>
																	<value>10</value>
																</Parameters>
															</Write>
														</Actions>
													</AbscissasUpperbound>
													<Next></Next>
													<Finish></Finish>
												</Actions>
											</Content>
										</Actions>
									</AddNewCurve>
									<SelectCurve>
										<Parameters>
											<curveName>Series 1</curveName>
										</Parameters>
									</SelectCurve>
									<DeleteCurve>
										<Actions>
											<ReadPopUpMessage>
												<Actions>
													<ElementAt>
														<Parameters>
															<index>0</index>
														</Parameters>
														<ExpectedResult>Do you want to remove the selected series?</ExpectedResult>
													</ElementAt>
												</Actions>
											</ReadPopUpMessage>
											<ClosePopUp>
												<Parameters>
													<result>true</result>
												</Parameters>
											</ClosePopUp>
										</Actions>
									</DeleteCurve>
									<AddNewCurve>
										<Actions>
											<Content>
												<Actions>
													<SourceType>
														<Parameters>
															<typeName>Linear function</typeName>
														</Parameters>
													</SourceType>
													<SetA>
														<Actions>
															<Write>
																<Parameters>
																	<value>1</value>
																</Parameters>
															</Write>
														</Actions>
													</SetA>
													<SetB>
														<Actions>
															<Write>
																<Parameters>
																	<value>1</value>
																</Parameters>
															</Write>
														</Actions>
													</SetB>
													<AbscissasLowerbound>
														<Actions>
															<Write>
																<Parameters>
																	<value>2e2</value>
																</Parameters>
															</Write>
														</Actions>
													</AbscissasLowerbound>
													<AbscissasUpperbound>
														<Actions>
															<Write>
																<Parameters>
																	<value>300</value>
																</Parameters>
															</Write>
														</Actions>
													</AbscissasUpperbound>
													<Next></Next>
													<TabChartType>
														<Actions>
															<ManageSeriesTable>
																<Actions>
																	<ReadCellValue>
																		<Parameters>
																			<row>1</row>
																			<col>1</col>
																		</Parameters>
																		<ExpectedResult>200</ExpectedResult>
																	</ReadCellValue>
																	<ReadCellValue>
																		<Parameters>
																			<row>1</row>
																			<col>2</col>
																		</Parameters>
																		<ExpectedResult>201</ExpectedResult>
																	</ReadCellValue>
																</Actions>
															</ManageSeriesTable>
															<NumberOfPointInChart>
																<ExpectedResult>10</ExpectedResult></NumberOfPointInChart>
														</Actions>
													</TabChartType>
													<Finish></Finish>
												</Actions>
											</Content>
										</Actions>
									</AddNewCurve>
								</Actions>
							</Content>
						</Actions>
					</CurveModule>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>