<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Do the settings of the Exponential curve </testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<CurveModule>
						<Actions>
							<Content>
								<Actions>
									<AddNewCurve>
										<Actions>
											<Content>
												<Actions>
													<SourceType>
														<Parameters>
															<typeName>Power function</typeName>
														</Parameters>
													</SourceType>
													<FunctionEquation>
														<Actions>
															<Read>
																<ExpectedResult>y = A * x^B</ExpectedResult>
															</Read>
														</Actions>
													</FunctionEquation>
													<SetA>
														<Actions>
															<Write>
																<Parameters>
																	<value>1</value>
																</Parameters>
															</Write>
														</Actions>
													</SetA>
													<SetB>
														<Actions>
															<Write>
																<Parameters>
																	<value>1</value>
																</Parameters>
															</Write>
														</Actions>
													</SetB>
													<AbscissasLowerbound>
														<Actions>
															<Write>
																<Parameters>
																	<value>1</value>
																</Parameters>
															</Write>
														</Actions>
													</AbscissasLowerbound>
													<AbscissasUpperbound>
														<Actions>
															<Write>
																<Parameters>
																	<value>10</value>
																</Parameters>
															</Write>
														</Actions>
													</AbscissasUpperbound>
													<Next>
													</Next>
													<Finish>
													</Finish>
												</Actions>
											</Content>
										</Actions>
									</AddNewCurve>
									<NumberOfPointsInChart>
										<ExpectedResult>10</ExpectedResult>
									</NumberOfPointsInChart>
								</Actions>
							</Content>
						</Actions>
					</CurveModule>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>