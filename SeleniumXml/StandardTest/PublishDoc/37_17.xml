<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Publish a file with having big size of the given limit of iis (attribute 'List Files')</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrateur</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_WriteView</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>Test 25_10_17</path>
											</nodes>
										</Parameters>
									</SelectEntity>
								</Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<Edit>
						<Actions>
							<Content>
								<Actions>
									<PublishDocumentFile>
										<Parameters>
											<attrId>85</attrId>
											<pathFile>#DocPath#BigFile1.txt</pathFile>
										</Parameters>
									</PublishDocumentFile>
								</Actions>
							</Content>
						</Actions>
					</Edit>
					<PopUp>
						<Actions>
							<ReadPopUpMessage>
								<Actions>
									<ElementAt>
										<Parameters>
											<index>0</index>
										</Parameters>
										<ExpectedResult>The size of a file must not exceed 28.61 Mb. The following file(s) exceed this limit:|The size of a file must not exceed 28.61 MB. The following file(s) exceed this limit:</ExpectedResult>
									</ElementAt>
									<ElementAt>
										<Parameters>
											<index>1</index>
										</Parameters>
										<ExpectedResult>BigFile1.txt: 169.63 Mb|BigFile1.txt: 169.63 MB</ExpectedResult>
									</ElementAt>
								</Actions>
							</ReadPopUpMessage>
						</Actions>
					</PopUp>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>