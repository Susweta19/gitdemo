<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Publish  png file on Standard Block's One File attribute with having SpecialCharacter in Title</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrateur</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_WriteView</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>Test 25_10_17</path>
											</nodes>
										</Parameters>
									</SelectEntity>
								</Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<Edit>
						<Actions>
							<Content>
								<Actions>
									<PublishDocumentFile>
										<Parameters>
											<attrId>86</attrId>
											<pathFile>#DocPath#PNG\test.png</pathFile>
										</Parameters>
									</PublishDocumentFile>
									<EnableDisableDocumentVisualization>
										<Parameters>
											<attrId>86</attrId>
										</Parameters>
									</EnableDisableDocumentVisualization>
								</Actions>
							</Content>
							<Ok>
								<Parameters>
									<validate>true</validate>
								</Parameters>
							</Ok>
						</Actions>
					</Edit>
					<ReadForm>
						<Actions>
							<Read>
								<WEGLink>
									<Parameters>
										<attrId>86</attrId>
									</Parameters>
									<Actions>
										<GetText>
											<ExpectedResult>test.png</ExpectedResult>
										</GetText>
									</Actions>
								</WEGLink>
							</Read>
						</Actions>
					</ReadForm>
					<Edit>
						<Actions>
							<Content>
								<Actions>
									<Delete>
										<Parameters>
											<attrId>86</attrId>
										</Parameters>
									</Delete>
								</Actions>
							</Content>
							<Ok>
								<Parameters />
							</Ok>
						</Actions>
					</Edit>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>