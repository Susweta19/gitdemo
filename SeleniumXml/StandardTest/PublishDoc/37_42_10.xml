									<ElementAt>
										<Parameters>
											<index>0</index>
										</Parameters>
										<Actions>
											<Click></Click>
											<Sleep>
												<Parameters>
													<timeOut>3</timeOut>
												</Parameters>
											</Sleep>
										</Actions>
									</ElementAt>
								</Actions>
							</ReadListLink>
						</Actions>
					</ReadForm>
					<Edit>
						<Actions>
							<Content>
								<Actions>
									<Delete>
										<Parameters>
											<attrId>86</attrId>
										</Parameters>
									</Delete>
								</Actions>
							</Content>
							<Ok>
								<Parameters>
									<validate>true</validate>
								</Parameters>
							</Ok>
						</Actions>
					</Edit>
					<DownloadFileChecking>
						<Parameters>
							<DownloadDirectory>#DownloadDirectory#</DownloadDirectory>
							<fileName>[].png</fileName>
						</Parameters>
						<ExpectedResult>true</ExpectedResult>
					</DownloadFileChecking>
				</Actions>
