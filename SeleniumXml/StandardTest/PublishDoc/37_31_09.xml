<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Publish  png file on Standard Block's List Files attribute with having SpecialCharacter in Title</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrateur</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_WriteView</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>Test 25_10_17</path>
											</nodes>
										</Parameters>
									</SelectEntity>
								</Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<Edit>
						<Actions>
							<Content>
								<Actions>
									<PublishDocumentFile>
										<Parameters>
											<attrId>85</attrId>
											<pathFile>#DocPath#PNG\,.png</pathFile>
										</Parameters>
									</PublishDocumentFile>
									<EnableDisableDocumentVisualization>
										<Parameters>
											<attrId>85</attrId>
											<index>99</index>
										</Parameters>
									</EnableDisableDocumentVisualization>
								</Actions>
							</Content>
							<Ok>
								<Parameters>
									<validate>true</validate>
								</Parameters>
							</Ok>
						</Actions>
					</Edit>
					<ReadForm>
						<Actions>
							<ReadListLink>
								<Parameters>
									<attrId>85</attrId>
								</Parameters>
								<Actions>
									<LastElement>
										<Actions>
											<GetText>
												<ExpectedResult>,.png</ExpectedResult>
											</GetText>
										</Actions>
									</LastElement>
								</Actions>
							</ReadListLink>
						</Actions>
					</ReadForm>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>