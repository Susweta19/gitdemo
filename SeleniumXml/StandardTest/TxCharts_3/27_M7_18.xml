<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Do the settings of the Exponential curve </testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<CurveModule>
						<Actions>
							<Content>
								<Actions>
									<AddNewCurve>
										<Actions>
											<Content>
												<Actions>
													<SourceType>
														<Parameters>
															<typeName>Polynomial function</typeName>
														</Parameters>
													</SourceType>
													<Degree>
														<Actions>
															<Write>
																<Parameters>
																	<value>2</value>
																</Parameters>
															</Write>
														</Actions>
													</Degree>
													<numberOfCoefficientInputBox>
														<ExpectedResult>3</ExpectedResult>
													</numberOfCoefficientInputBox>
													<AbscissasLowerbound>
														<Actions>
															<Write>
																<Parameters>
																	<value>1</value>
																</Parameters>
															</Write>
														</Actions>
													</AbscissasLowerbound>
													<AbscissasUpperbound>
														<Actions>
															<Write>
																<Parameters>
																	<value>10</value>
																</Parameters>
															</Write>
														</Actions>
													</AbscissasUpperbound>
													<Next>
													</Next>
													<TabChartType>
														<Actions>
															<NumberOfPointInChart>
																<ExpectedResult>10</ExpectedResult>
															</NumberOfPointInChart>
															<ManageSeriesTable>
																<Actions>
																	<ReadCellValue>
																		<Parameters>
																			<row>1</row>
																			<col>2</col>
																		</Parameters>
																		<ExpectedResult>3</ExpectedResult>
																	</ReadCellValue>
																</Actions>
															</ManageSeriesTable>
														</Actions>
													</TabChartType>
													<Finish>
													</Finish>
												</Actions>
											</Content>
										</Actions>
									</AddNewCurve>
									<NewChart>
										<Actions>
											<ReadPopUpMessage>
												
												<Actions>
													<ElementAt>
														<Parameters>
															<index>0</index>
														</Parameters>
														<ExpectedResult>Do you want to reset the workspace?</ExpectedResult>
													</ElementAt>
												</Actions>
											</ReadPopUpMessage>
											<ClosePopUp>
												<Parameters>
													<result>true</result>
												</Parameters>
											</ClosePopUp>
										</Actions>
									</NewChart>
								</Actions>
							</Content>
						</Actions>
					</CurveModule>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>