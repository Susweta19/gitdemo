																		</Parameters>
																		<ExpectedResult>Upper bound should be greater than lower bound</ExpectedResult>
																	</ElementAt>
																</Actions>
															</ReadPopUpMessage>
															<ClosePopUp>
																<Parameters>
																	<result>true</result>
																</Parameters>
															</ClosePopUp>
														</Actions>
													</ManagePopUp>
												</Actions>
											</Content>
										</Actions>
									</AddNewCurve>
