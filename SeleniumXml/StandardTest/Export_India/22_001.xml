<?xml version="1.0" encoding="utf-8"?>
<!--Check the ability to open the 'Export' window-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the ability to open the 'Export' window</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Export</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
            </Actions>
          </NavigationTree>
          <Export>
            <Actions>
              <ReadHeader>
              	<ExpectedResult>Export</ExpectedResult></ReadHeader>
              <Content>
                <Actions>
                  <Filtre>
                    <Actions>
                      <Read>
                        <ExpectedResult>TEST_Export</ExpectedResult>
                      </Read>
                    </Actions>
                  </Filtre>
                  <Exports>
                    <Actions>
                      <Read>
                        <ExpectedResult>[Excel] Standard Export</ExpectedResult>
                      </Read>
                    </Actions>
                  </Exports>
                  <ReadAllFields>
                  	<Actions>
                  		<ElementAt>
                  			<Parameters>
                  				<index>0</index>
                  			</Parameters>
                  			<ExpectedResult>Filter:</ExpectedResult>
                  		</ElementAt>
                  		<ElementAt>
                  			<Parameters>
                  				<index>1</index>
                  			</Parameters>
                  			<ExpectedResult>Exports:</ExpectedResult>
                  		</ElementAt>
                  		<ElementAt>
                  			<Parameters>
                  				<index>2</index>
                  			</Parameters>
                  			<ExpectedResult>Objects</ExpectedResult>
                  		</ElementAt>
                  		<ElementAt>
                  			<Parameters>
                  				<index>3</index>
                  			</Parameters>
                  			<ExpectedResult>Data</ExpectedResult>
                  		</ElementAt>
                  		<ElementAt>
                  			<Parameters>
                  				<index>4</index>
                  			</Parameters>
                  			<ExpectedResult>Compress the data file</ExpectedResult>
                  		</ElementAt></Actions>
                  </ReadAllFields>
                </Actions>
              </Content>
              <IsButtonPresent>
              	<Parameters>
              		<buttonName>Export</buttonName>
              	</Parameters>
              	<ExpectedResult>true</ExpectedResult>
              </IsButtonPresent>
              <IsButtonPresent>
              	<Parameters>
              		<buttonName>Close</buttonName>
              	</Parameters>
              	<ExpectedResult>true</ExpectedResult>
              </IsButtonPresent>
            </Actions>
          </Export>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>