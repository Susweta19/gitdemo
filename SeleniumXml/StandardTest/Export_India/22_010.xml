<?xml version="1.0" encoding="utf-8"?>
<!--Check the ability to open the 'Export' window-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check The Functionlity of Search in Object List</testDescription>
        <testBugRef>16258</testBugRef>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Export</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
            </Actions>
          </NavigationTree>
          <Export>
            <Actions>
              <Content>
                <Actions>
                	<ObjectsTree>
                		<Actions>
                			<Search>
                				<Parameters>
                					<research>N</research>
                					<popup>false</popup>
                				</Parameters>
                			</Search></Actions>
                	</ObjectsTree>
                	<ManagePopUp>
                		<Actions>
                			<ReadPopUpMessage>
                				<Actions>
                					<ElementAt>
                						<Parameters>
                							<index>0</index>
                						</Parameters>
                						<ExpectedResult>More information is required in order to carry out a search.</ExpectedResult>
                					</ElementAt></Actions>
                			</ReadPopUpMessage>
                			<CloseInformationPopup>
                				<Parameters>
                					<currentFrame>true</currentFrame></Parameters>
                			</CloseInformationPopup></Actions>
                	</ManagePopUp>
                	<ObjectsTree>
                		<Actions>
                			<Search>
                				<Parameters>
                					<research>Test</research>
                				</Parameters>
                			</Search>
                			<EntitytPresence>
                				<Parameters>
                					<nodes>
                						<path>TestIndia</path></nodes>
                					<presence>true</presence>
                				</Parameters>
                			</EntitytPresence></Actions>
                	</ObjectsTree>
                </Actions>
              </Content>
              <Close></Close>
            </Actions>
          </Export>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>