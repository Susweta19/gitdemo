<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the availability of a 'When data is written' Model Application using 3 inputs</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>ModelApplications</value>
                    </Parameters>
                  </Select>
                  <Read>
                  	<ExpectedResult>ModelApplications</ExpectedResult></Read>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters><nodes><path>New</path></nodes></Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <Write>
                    <DTText>
                      <Parameters>
                        <id>477</id>
                        <value>
                          <value>10</value>
                        </value>
                      </Parameters>
                    </DTText>
                  </Write>
                  <Write>
                    <DTText>
                      <Parameters>
                        <id>487</id>
                        <value>
                          <value>20</value>
                        </value>
                      </Parameters>
                    </DTText>
                  </Write>
                  <Write>
                    <DTText>
                      <Parameters>
                        <id>486</id>
                        <value>
                          <value>30</value>
                        </value>
                      </Parameters>
                    </DTText>
                  </Write>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <ReadForm>
            <Actions>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>485</attrId>
                  </Parameters>
                  <ExpectedResult>
                    <min>60</min>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>