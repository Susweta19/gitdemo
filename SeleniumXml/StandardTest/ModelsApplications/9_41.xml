<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription> Go to TEEXMA front end, in ReadForm and WriteForm, check the availability of the application button on the toolbar, The application button will always be present no matter in ReadForm or WriteForm, Click on the button, the value of the Attribute 'TEST_Launch_Main toolbar' will be 66</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>ModelApplications</value>
                    </Parameters>
                  </Select>
                  <Read>
                  	<ExpectedResult>ModelApplications</ExpectedResult></Read>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters><nodes><path>NewWRF</path></nodes></Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <MainToolbar>
            <Parameters>
              <modelName>Main toolbar</modelName>
            </Parameters>
          </MainToolbar>
          <ReadForm>
            <Actions>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>475</attrId>
                  </Parameters>
                  <ExpectedResult>
                    <min>66</min>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>