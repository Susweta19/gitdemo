<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Go to TEEXMA WEB, select OT 'ModelApplications'. Turn to WriteForm, for the Object 'New11', select the Tab 'Tab_3'. Tick a target Object for the Attribute 'Single List for Model App' then turn back to ReadForm. Select another Object then turn back to the Object 'New11'. When the Object 'New11' has been selected, its banner will be modified to the format in the related .html files(..\Customer Resources\Models\Banner\Banner_HTML)</testDescription>
        <testBugRef>12</testBugRef>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>ModelApplications</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters><nodes><path>New</path></nodes></Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <ChangeTab>
                    <Parameters>
                      <tabIndex>2</tabIndex>
                    </Parameters>
                  </ChangeTab>
                  <GetLink>
                    <Parameters>
                      <attrId>493</attrId>
                    </Parameters>
                    <Actions>
                      <UncheckAll>
                      </UncheckAll>
                      <SelectEntityBox>
                        <Parameters><nodes><path>Banner</path></nodes></Parameters>
                        <Actions>
                          <Tick>
                          </Tick>
                          <Read>
                            <ExpectedResult>
																true
															</ExpectedResult>
                          </Read>
                        </Actions>
                      </SelectEntityBox>
                    </Actions>
                  </GetLink>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <ReadBanner>
            <ExpectedResult>New (Created by Administrator. Date: 20/06/2014)</ExpectedResult>
          </ReadBanner>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>