<?xml version="1.0" encoding="utf-8"?>
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Search of specific string without fast mode
				</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<SearchBox>
						<Parameters>
							<research>new</research>
							<byClick>true</byClick>
						</Parameters>
						<Actions>
							<Content>

								<Actions>
									<Results>
										<Actions>
											<EntitytPresence>
												<Parameters>
													<nodes>
														<path>New</path>
													</nodes>
													<presence>true</presence>
												</Parameters>
											</EntitytPresence>
										</Actions>
									</Results>
									<AllOfTheFollowingWords>
										<Actions>
											<Write>
												<Parameters>
													<value>ew</value>
												</Parameters>
											</Write>
											<PressEnter></PressEnter></Actions>
									</AllOfTheFollowingWords>
									<Results>
										<Actions>
											<EntitytPresence>
												<Parameters>
													<nodes>
														<path>New</path></nodes>
													<presence>false</presence>
												</Parameters>
											</EntitytPresence></Actions>
									</Results>
								</Actions>
							</Content>
							<Close />
						</Actions>
					</SearchBox>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>