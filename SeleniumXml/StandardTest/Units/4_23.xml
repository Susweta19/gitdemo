<?xml version="1.0" encoding="utf-8"?>
<!--Check availability of convertion of MMM value(Scientific Display Type) without bound-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check availability of convertion of MMM value(Scientific Display Type) without bound</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>People</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>Administrator</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>1103</id>
                        <value>
                          <min>10</min>
                          <max>11</max>
                          <mean>10.5</mean>
                          <unit>
                            <unitName>°F</unitName>
                          </unit>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>1103</id>
                        <value>
                          <unit>
                            <unitName>°C</unitName>
                          </unit>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                  <Read>
                    <DTDecimalValue>
                      <Parameters>
                        <id>1103</id>
                      </Parameters>
                      <ExpectedResult>
                        <min>-12.2222222222222</min>
                        <max>-11.6666666666667</max>
                        <mean>-11.9444444444444</mean>
                        <unit>
                          <unitName>°C</unitName>
                        </unit>
                      </ExpectedResult>
                    </DTDecimalValue>
                  </Read>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <!--Automatically and manually value will be changed randomly-->
          <ReadForm>
            <Actions>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>1103</attrId>
                  </Parameters>
                  <ExpectedResult>
                    <min>1,0000000000e+001|1.0000000000e+001</min>
                    <max>1,1000000000e+001|1.1000000000e+001</max>
                    <mean>1,0500000000e+001|1.0500000000e+001</mean>
                    <unit>
                      <unitName>°F</unitName>
                    </unit>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
              <ChangeUnit>
                <Parameters>
                  <attrId>1103</attrId>
                  <unit>°C</unit>
                </Parameters>
              </ChangeUnit>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>1103</attrId>
                  </Parameters>
                  <ExpectedResult>
                    <min>-1,222222222e+001|-1.222222222e+001</min>
                    <max>-1,166666667e+001|-1.166666667e+001</max>
                    <mean>-1,194444444e+001|-1.194444444e+001</mean>
                    <unit>
                      <unitName>°C</unitName>
                    </unit>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>