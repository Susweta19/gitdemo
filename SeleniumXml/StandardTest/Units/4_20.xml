<?xml version="1.0" encoding="utf-8"?>
<!---->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check availability of convertion of Min-Max-Mean value( Scientific Display Type) with bound</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>People</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>Administrator</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>1100</id>
                        <value>
                          <min>20</min>
                          <max>63</max>
                          <mean>20.5</mean>
                          <unit>
                            <unitName>°F</unitName>
                          </unit>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>1100</id>
                        <value>
                          <unit>
                            <unitName>°C</unitName>
                          </unit>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                  <Read>
                    <DTDecimalValue>
                      <Parameters>
                        <id>1100</id>
                      </Parameters>
                      <ExpectedResult>
                        <min>-6.66666666666667</min>
                        <max>17.2222222222222</max>
                        <mean>-6.38888888888889</mean>
                        <unit>
                          <unitName>°C</unitName>
                        </unit>
                      </ExpectedResult>
                    </DTDecimalValue>
                  </Read>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <ReadForm>
            <Actions>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>1100</attrId>
                  </Parameters>
                  <ExpectedResult>
                    <min>-6,666666667e+000|-6.666666667e+000</min>
                    <max>1,7222222222e+001|1.7222222222e+001</max>
                    <mean>-6,388888889e+000|-6.388888889e+000</mean>
                    <unit>
                      <unitName>°C</unitName>
                    </unit>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
              <ChangeUnit>
                <Parameters>
                  <attrId>1100</attrId>
                  <unit>°F</unit>
                </Parameters>
              </ChangeUnit>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>1100</attrId>
                  </Parameters>
                  <ExpectedResult>
                    <min>2,0000000000e+001|2.0000000000e+001</min>
                    <max>6,3000000000e+001|6.3000000000e+001</max>
                    <mean>2,0500000000e+001|2.0500000000e+001</mean>
                    <unit>
                      <unitName>°F</unitName>
                    </unit>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>