<?xml version="1.0" encoding="utf-8"?>
<!--Check availability of convertion of single value(Advance Display Type) without bound-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check availability of convertion of single value(Advance Display Type) without bound</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>People</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>Administrator</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>1093</id>
                        <value>
                          <min>12</min>
                          <unit>
                            <unitName>cm</unitName>
                          </unit>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>1093</id>
                        <value>
                          <unit>
                            <unitName>m</unitName>
                          </unit>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                  <Read>
                    <DTDecimalValue>
                      <Parameters>
                        <id>1093</id>
                      </Parameters>
                      <ExpectedResult>
                        <min>0.12</min>
                        <unit>
                          <unitName>m</unitName>
                        </unit>
                      </ExpectedResult>
                    </DTDecimalValue>
                  </Read>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <ReadForm>
            <Actions>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>1093</attrId>
                  </Parameters>
                  <ExpectedResult>
                    <min>0,1200|0.1200</min>
                    <unit>
                      <unitName>m</unitName>
                    </unit>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
              <ChangeUnit>
                <Parameters>
                  <attrId>1093</attrId>
                  <unit>cm</unit>
                </Parameters>
              </ChangeUnit>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>1093</attrId>
                  </Parameters>
                  <ExpectedResult>
                    <min>12,0000|12.0000</min>
                    <unit>
                      <unitName>cm</unitName>
                    </unit>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>