<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Fill in the Attribute 'float_1' with a Decimal value then Click on the tab 'Events_2' in read form. Check the values of the Attributes 'FloatToStr_Answer'</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Utils</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <ChangeTab>
                    <Parameters>
                      <tabIndex>1</tabIndex>
                    </Parameters>
                  </ChangeTab>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>152</id>
                        <!--we write the value in float_1-->
                        <value>
                          <min>13.23</min>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <Sleep>
            <Parameters>
              <timeOut>2</timeOut>
            </Parameters>
          </Sleep>
          <ReadForm>
            <Actions>
              <ChangeTab>
                <Parameters>
                  <tabIndex>1</tabIndex>
                </Parameters>
              </ChangeTab>
              <ChangeTab>
                <Parameters>
                  <tabIndex>0</tabIndex>
                </Parameters>
              </ChangeTab>
              <ChangeTab>
                <Parameters>
                  <tabIndex>1</tabIndex>
                </Parameters>
              </ChangeTab>
              <Sleep>
                <Parameters>
                  <timeOut>5</timeOut>
                </Parameters>
              </Sleep>
              <Read>
                <DTText>
                  <Parameters>
                    <attrId>153</attrId>
                  </Parameters>
                  <ExpectedResult>
                    <value>13,23|13.23</value>
                  </ExpectedResult>
                </DTText>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>