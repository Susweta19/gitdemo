<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the data placement for Excel Template file for Document_Email attribute</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrateur</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>Test&#160;Extraction</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                	<SelectEntities>
                		<Parameters>
                			<collection>
                				<collection>
                					<path>Entité Test Word/Excel</path></collection></collection>
                		</Parameters>
                	</SelectEntities>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <MainToolbar>
          	<Parameters>
          		<modelName>ExtractionMainToolBar</modelName>
          	</Parameters>
          </MainToolbar>
          <ReturnExtraction>
          	<Actions>
          		<Content>
          			<Actions>
          				<Extractions>
          					<Actions>
          						<Select>
          							<Parameters>
          								<value>[Excel]&#160;Excel_Generalites_Document_Email</value>
          							</Parameters>
          						</Select></Actions>
          				</Extractions>
          				<Extract>
          					<Parameters></Parameters>
          					<Actions></Actions>
          				</Extract></Actions>
          		</Content></Actions>
          </ReturnExtraction>
          <Sleep>
          	<Parameters>
          		<timeOut>10</timeOut>
          	</Parameters>
          </Sleep>
          <DownloadFileChecking>
          									<Parameters>
          										<DownloadDirectory>#DownloadDirectory#</DownloadDirectory>
          										<fileName>Excel_Generalites_Document_Email</fileName>
          										<contains>
          											true
          										</contains>
          									</Parameters>
          									<ExpectedResult>
          										true
          									</ExpectedResult>
          								</DownloadFileChecking>
          								<ManageDownloadedFiles>
          	<Actions>
          		<GetLastDownloadedFile>
          			<Parameters>
          				<DownloadDirectory>#DownloadDirectory#</DownloadDirectory>
          			</Parameters>
          			<Hashtag>#LastDownloadedFile#</Hashtag>
          		</GetLastDownloadedFile>
          		<ReadExcelFileFromZeroIndex>
          			<Parameters>
          				<filePath>#LastDownloadedFile#</filePath>
          				<sheetName>Test</sheetName>
          				<row>2</row>
          				<uptoCol>0</uptoCol>
          			</Parameters>
          			<Actions>
          				<ElementAt>
          					<Parameters>
          						<index>0</index>
          					</Parameters>
          					<ExpectedResult>adrien.etienne@bassetti.fr</ExpectedResult>
          				</ElementAt></Actions>
          		</ReadExcelFileFromZeroIndex></Actions>
          </ManageDownloadedFiles>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>