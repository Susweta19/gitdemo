<?xml version="1.0" encoding="utf-8"?>
<!-- open a Choice Guide from a blank template-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the ability to open a Choice Guide from a blank template.</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface><Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
				<ChoiceGuide>
					<Actions>
						<Content>
							<Actions>
								<ChoiceGuideFromTemplate>
									<Parameters>
										<template>TEST_ChoiceGuidesSetting</template>
									</Parameters>
									<Actions />
								</ChoiceGuideFromTemplate></Actions>
						</Content></Actions>
				</ChoiceGuide></Actions>
			</MainWindow></Actions></TxInterface></Actions>