<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Open TXTableView through TableView Widget </testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associative test start</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <ExpandNavigationTree />
          <NavigationTree>
            <Actions>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>Widget</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <TableViewInNewTab>
            <Parameters>
              <ByClickingNewTab>true</ByClickingNewTab>
            </Parameters>
            <Actions>
              <Content>
                <Actions>
                  <Refresh />
                </Actions>
              </Content>
              <Close>
                <Parameters />
              </Close>
            </Actions>
          </TableViewInNewTab>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>