<?xml version="1.0" encoding="utf-8"?>
<!--Check the availability of the 'MultiCriteria Selection' button-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Read the french translation of all buttons in the Choice Guide window</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
          <langage>French</langage>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
        	<LinkPortal>
        		<Parameters>
        			<link>To Choice Guide 'CG_1'</link>
        		</Parameters>
        	</LinkPortal>
        	<ReturnChoiceGuide>
        		<Actions>
        			<ReadHeader>
        				<ExpectedResult>Guide de Choix</ExpectedResult></ReadHeader>
        			<IsButtonPresent>
        				<Parameters>
        					<buttonName>Sélectionner...</buttonName>
        				</Parameters>
        				<ExpectedResult>true</ExpectedResult>
        			</IsButtonPresent>
        			<IsButtonPresent>
        				<Parameters>
        					<buttonName>Réinitialiser</buttonName>
        				</Parameters>
        				<ExpectedResult>true</ExpectedResult>
        			</IsButtonPresent>
        			<IsButtonPresent>
        				<Parameters>
        					<buttonName>Extraire</buttonName>
        				</Parameters>
        				<ExpectedResult>true</ExpectedResult>
        			</IsButtonPresent>
        			
        			<IsButtonPresent>
        				<Parameters>
        					<buttonName>Imprimer</buttonName>
        				</Parameters>
        				<ExpectedResult>true</ExpectedResult>
        			</IsButtonPresent>
        			<IsButtonPresent>
        				<Parameters>
        					<buttonName>Exporter</buttonName>
        				</Parameters>
        				<ExpectedResult>true</ExpectedResult>
        			</IsButtonPresent>
        		</Actions>
        	</ReturnChoiceGuide>

        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>