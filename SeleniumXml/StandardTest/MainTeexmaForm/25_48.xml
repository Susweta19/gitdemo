<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the Export window in English Translation</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
					<langage>English</langage>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<ExpandNavigationTree></ExpandNavigationTree>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_ReadView</value>
										</Parameters>
									</Select></Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>New</path></nodes>
										</Parameters>
									</SelectEntity></Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<Export>
						<Actions>
							<ReadHeader>
								<ExpectedResult>Export</ExpectedResult></ReadHeader>
							<Content>
								<Actions>
									<Filtre>
										<Actions>
											<Read>
												<ExpectedResult>TEST_ReadView</ExpectedResult></Read></Actions>
									</Filtre>
									<Exports>
										<Actions>
											<Read>
												<ExpectedResult>[Excel] Standard Export</ExpectedResult></Read></Actions>
									</Exports>
									<ObjectsTree>
										<Actions>
											<IsButtonPresentByTitle>
												<Parameters>
													<buttonName>Display the entire tree</buttonName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsButtonPresentByTitle>
											<IsButtonPresentByTitle>
												<Parameters>
													<buttonName>Check all Objects</buttonName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsButtonPresentByTitle>
											<IsButtonPresentByTitle>
												<Parameters>
													<buttonName>Uncheck all Objects</buttonName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsButtonPresentByTitle>
											<IsButtonPresentByTitle>
												<Parameters>
													<buttonName>Import a preselection from a text or Excel file...</buttonName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsButtonPresentByTitle></Actions>
									</ObjectsTree>
									<DataTree>
										<Actions>
											<IsButtonPresentByTitle>
												<Parameters>
													<buttonName>View the checked Attributes</buttonName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsButtonPresentByTitle>
											<IsButtonPresentByTitle>
												<Parameters>
													<buttonName>Select all Attributes</buttonName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsButtonPresentByTitle>
											<Sleep>
												<Parameters>
													<timeOut>1</timeOut>
												</Parameters>
											</Sleep>
											<IsButtonPresentByTitle>
												<Parameters>
													<buttonName>Deselect all Attributes</buttonName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsButtonPresentByTitle></Actions>
									</DataTree></Actions>
							</Content></Actions>
					</Export>
					<IsButtonNamePresent>
						<Parameters>
							<buttonName>Export</buttonName>
						</Parameters>
						<ExpectedResult>true</ExpectedResult>
					</IsButtonNamePresent>
					<IsButtonNamePresent>
						<Parameters>
							<buttonName>Close</buttonName>
						</Parameters>
						<ExpectedResult>true</ExpectedResult>
					</IsButtonNamePresent>
				</Actions>
			</MainWindow></Actions></TxInterface></Actions>