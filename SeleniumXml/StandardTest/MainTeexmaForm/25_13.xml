<?xml version="1.0" encoding="utf-8"?>
<!--Check the availability of the 'Quit' button-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>check current user info</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associative test start</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <UserInformation>
            <Actions>
              <ElementAt>
                <Parameters>
                  <index>0</index>
                </Parameters>
                <ExpectedResult>Administrator</ExpectedResult>
              </ElementAt>
              <ElementAt>
                <Parameters>
                  <index>1</index>
                </Parameters>
                <ExpectedResult>Last connection: #CurrentDate#</ExpectedResult>
              </ElementAt>
            </Actions>
          </UserInformation>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>