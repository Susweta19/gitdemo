<?xml version="1.0" encoding="utf-8"?>
<!--Check the display mode of the window title-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Launch TxTableView through an arrow in widget before that need to modify the 5.asp file</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associative test start</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <ExpandNavigationTree></ExpandNavigationTree>
          <NavigationTree>
          	<Actions>
          		<EntityTree>
          			<Actions>
          				<SelectEntity>
          					<Parameters>
          						<nodes>
          							<path>Widget</path></nodes>
          					</Parameters>
          				</SelectEntity></Actions>
          		</EntityTree></Actions>
          </NavigationTree>
          <TableViewInWidget>
          	<Actions>
          		<Refresh></Refresh></Actions>
          </TableViewInWidget>
          
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>