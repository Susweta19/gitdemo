<?xml version="1.0" encoding="utf-8"?>
<!--Check the ability to Modify Password for the current User-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Change password</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associative test start</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <ChangePassword>
            <Parameters>
              <newPassword>1234</newPassword>
              <oldPassword>aaaaa</oldPassword>
              <ok>false</ok>
            </Parameters>
          </ChangePassword>
          <ChangePassword>
            <Parameters>
              <newPassword>bbbbb</newPassword>
              <oldPassword>aaaaa</oldPassword>
              <ok>true</ok>
            </Parameters>
          </ChangePassword>
          <ChangePassword>
            <Parameters>
              <newPassword>aaaaa</newPassword>
              <oldPassword>bbbbb</oldPassword>
              <ok>true</ok>
            </Parameters>
          </ChangePassword>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>