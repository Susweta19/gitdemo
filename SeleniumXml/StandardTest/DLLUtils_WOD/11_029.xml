<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Fill in the Attribute 'Decimal_2' with a Numerical value, then  in readForm. Check the values of the Attributes 'Date_Answer_1' and 'Date_Answer_2' </testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Utils</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>136</id>
                        <!--we write the value in Decimal2-->
                        <value>
                          <min>2,25</min>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <Sleep>
            <Parameters>
              <timeOut>2</timeOut>
            </Parameters>
          </Sleep>
          <ReadForm>
            <Actions>
              <GetRequiredDate>
                <Parameters>
                  <dayAfterOrBefore>2</dayAfterOrBefore>
                </Parameters>
                <Hashtag>#DateAfterDays_2#</Hashtag>
              </GetRequiredDate>
              <ConvertStringToDTDate>
                <Parameters>
                  <date>#DateAfterDays_2#</date>
                </Parameters>
                <Hashtag>#DateAfterDays_2#</Hashtag>
              </ConvertStringToDTDate>
              <Read>
                <DTDate>
                  <Parameters>
                    <attrId>142</attrId>
                  </Parameters>
                  <ExpectedResult>
                    <value>#DateAfterDays_2#</value>
                  </ExpectedResult>
                </DTDate>
              </Read>
              <IsValueContains>
                <Parameters>
                  <attrId>147</attrId>
                  <value>#DateAfterDays_2#</value>
                </Parameters>
                <ExpectedResult>true</ExpectedResult>
              </IsValueContains>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>