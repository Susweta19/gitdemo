<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the ability to import a Requirement Set from a XML file</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associative test start</Message>
    <Actions>
      <Login>
        <Message>Login</Message>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <OpenRequirementSet>
            <Actions>
              <Content>
                <Actions>
                  <Browse>
                    <Parameters>
                      <pathFile>#DocPath#cg.xml</pathFile>
                    </Parameters>
                    <Actions>
                      <Close>
                      </Close>
                    </Actions>
                  </Browse>
                </Actions>
              </Content>
            </Actions>
          </OpenRequirementSet>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>