<?xml version="1.0" encoding="utf-8"?>
<!--Prasenjit-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Select the RS '{Boolean}' then launch calculation--if the 'true' radiobutton is ticked :the 7 objects ({Boolean} contains at least one true value) will be sorted out;if the 'false' radiobutton is ticked :the 8 objects ({Boolean} contains at least one false value) will be sorted out;</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associative test start</Message>
    <Actions>
      <Login>
        <Message>Login</Message>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <OpenRequirementSet>
            <Actions>
              <Content>
                <Actions>
                  <ObjectType>
                    <Parameters>
                      <objectType>TEST_MulticriteriaSelection</objectType>
                    </Parameters>
                  </ObjectType>
                  <RequirementSet>
                    <Parameters>
                      <requirementSet>{Boolean}</requirementSet>
                    </Parameters>
                  </RequirementSet>
                  <Open>
                    <Parameters>
                      <popUp>false</popUp>
                    </Parameters>
                    <Actions>
                      <Content>
                        <Actions>
                          <CriteriaTab>
                            <Actions>
                              <Content>
                                <Actions>
                                  <LaunchCalculation>
                                    <Actions>
                                      <Close />
                                    </Actions>
                                  </LaunchCalculation>
                                </Actions>
                              </Content>
                            </Actions>
                          </CriteriaTab>
                        </Actions>
                      </Content>
                    </Actions>
                  </Open>
                </Actions>
              </Content>
            </Actions>
          </OpenRequirementSet>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>