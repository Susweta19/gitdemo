<?xml version="1.0" encoding="utf-8"?>
<!--Prasenjit-->
<!--we should check number results without clicking -->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check in the Detailed results table--for 'To one of' ticked criterias : the result Objects will be extendable; when extended, the target Object will be displayed with the Adequacy and the criteria Attribute value also displayed;the linked Objects will be displayed in the 'Link_Single_Direct' column;the Adequacy will be the same as the one of the linked target Object;for 'None' ticked criterias : the Adequacy will all be 100%, the linked Objects will be displayed in the 'Link_Single_Direct' column</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associative test start</Message>
    <Actions>
      <Login>
        <Message>Login</Message>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <OpenRequirementSet>
            <Actions>
              <Content>
                <Actions>
                  <ObjectType>
                    <Parameters>
                      <objectType>TEST_MulticriteriaSelection</objectType>
                    </Parameters>
                  </ObjectType>
                  <RequirementSet>
                    <Parameters>
                      <requirementSet>Single Link</requirementSet>
                    </Parameters>
                  </RequirementSet>
                  <Open>
                    <Parameters>
                      <popUp>false</popUp>
                    </Parameters>
                    <Actions>
                      <Content>
                        <Actions>
                          <ResultsTab>
                            <Actions>
                              <Close />
                            </Actions>
                          </ResultsTab>
                        </Actions>
                      </Content>
                    </Actions>
                  </Open>
                </Actions>
              </Content>
            </Actions>
          </OpenRequirementSet>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>