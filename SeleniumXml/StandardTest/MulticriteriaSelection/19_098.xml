<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Launch a Multicriteria Selection then view the detailed results. Click on the button--The Exportation window will appear :For Filter dropdown list, the current OT will be selected by default;For Exporatations dropdown list, the '[Excel]For_TreeView' will be selected by default;All Objects of the selected OT will be ticked by default;No Attribute will be ticked by default,;The 'Compress the data file' checkbox will not be ticked by default</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associative test start</Message>
    <Actions>
      <Login>
        <Message>Login</Message>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_MulticriteriaSelection</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
            </Actions>
          </NavigationTree>
          <MultiCriteria>
            <Actions>
              <Content>
                <Actions>
                  <ResultsTab>
                    <Actions>
                      <Content>
                        <Actions>
                          <Export>
                            <Actions>
                              <Content>
                                <Actions>
                                  <ObjectsTree>
                                    <Actions>
                                      <SelectEntityBox>
                                        <Parameters>
                                          <nodes>
                                            <path>New</path>
                                          </nodes>
                                        </Parameters>
                                        <Actions>
                                          <Tick />
                                        </Actions>
                                      </SelectEntityBox>
                                    </Actions>
                                  </ObjectsTree>
                                  <DataTree>
                                    <Actions>
                                      <SelectEntitiesBox>
                                        <Parameters>
                                          <collection>
                                            <collection>
                                              <path>Boolean</path>
                                            </collection>
                                            <collection>
                                              <path>Date</path>
                                            </collection>
                                          </collection>
                                        </Parameters>
                                        <Actions>
                                          <Tick />
                                        </Actions>
                                      </SelectEntitiesBox>
                                    </Actions>
                                  </DataTree>
                                  <Export />
                                </Actions>
                              </Content>
                            </Actions>
                          </Export>
                        </Actions>
                      </Content>
                    </Actions>
                  </ResultsTab>
                </Actions>
              </Content>
            </Actions>
          </MultiCriteria>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>