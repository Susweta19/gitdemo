<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Select an Attribute, then check the ability to delete a criterion using the 'Delete the criterion' button--The button will be available only if an Attribute containing a criteria has been selected (highlighted);Click on the button, the criteria of the selected Attribute will be deleted and reseted </testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associative test start</Message>
    <Actions>
      <Login>
        <Message>Login</Message>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <MultiCriteria>
            <Actions>
              <Content>
                <Actions>
                  <CriteriaTab>
                    <Actions>
                      <Content>
                        <Actions>
                          <ReqSetObjectType>
                            <Actions>
                              <SelectV2>
                                <Parameters>
                                  <value>TEST_MulticriteriaSelection</value>
                                </Parameters>
                              </SelectV2>
                            </Actions>
                          </ReqSetObjectType>
                          <Sleep>
                            <Parameters>
                              <timeOut>5</timeOut>
                            </Parameters>
                          </Sleep>
                          <DefineCriteria>
                            <Actions>
                              <Content>
                                <Actions>
                                  <ListOfAttributes>
                                    <Actions>
                                      <SelectEntity>
                                        <Parameters>
                                          <nodes>
                                            <path>Boolean</path>
                                          </nodes>
                                        </Parameters>
                                      </SelectEntity>
                                    </Actions>
                                  </ListOfAttributes>
                                  <BooleanCriterion>
                                    <Parameters>
                                      <value>
    																	true
    																</value>
                                    </Parameters>
                                  </BooleanCriterion>
                                  <DeleteTheCriterion />
                                </Actions>
                              </Content>
                            </Actions>
                          </DefineCriteria>
                        </Actions>
                      </Content>
                    </Actions>
                  </CriteriaTab>
                </Actions>
              </Content>
            </Actions>
          </MultiCriteria>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>