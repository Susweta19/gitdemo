<?xml version="1.0" encoding="utf-8"?>
<!--Prasenjit-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Lists/Links(example : TEST_Links/Lists)--For Single Links/Lists :check the 2 radiobuttons (To one of, None);check the availability of the "exclusivity" checkbox (deactivated when "None" radiobutton has been ticked or no radiobutton has been ticked)</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associative test start</Message>
    <Actions>
      <Login>
        <Message>Login</Message>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <MultiCriteria>
            <Actions>
              <Content>
                <Actions>
                  <CriteriaTab>
                    <Actions>
                      <Content>
                        <Actions>
                          <ReqSetObjectType>
                            <Actions>
                              <SelectV2>
                                <Parameters>
                                  <value>TEST_MulticriteriaSelection</value>
                                </Parameters>
                              </SelectV2>
                            </Actions>
                          </ReqSetObjectType>
                          <Sleep>
                            <Parameters>
                              <timeOut>5</timeOut>
                            </Parameters>
                          </Sleep>
                          <DefineCriteria>
                            <Actions>
                              <Content>
                                <Actions>
                                  <ListOfAttributes>
                                    <Actions>
                                      <SelectEntity>
                                        <Parameters>
                                          <nodes>
                                            <path>Link_Single_Direct</path>
                                          </nodes>
                                        </Parameters>
                                      </SelectEntity>
                                    </Actions>
                                  </ListOfAttributes>
                                  <SelectObjectslinked>
                                    <Parameters>
                                      <objectName>To one of</objectName>
                                    </Parameters>
                                  </SelectObjectslinked>
                                  <Recursivity>
                                    <Parameters>
                                      <linkAttribute>Link_Single_Direct</linkAttribute>
                                      <applyRecursivity>to all</applyRecursivity>
                                    </Parameters>
                                  </Recursivity>
                                </Actions>
                              </Content>
                            </Actions>
                          </DefineCriteria>
                        </Actions>
                      </Content>
                    </Actions>
                  </CriteriaTab>
                </Actions>
              </Content>
            </Actions>
          </MultiCriteria>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>