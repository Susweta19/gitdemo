<?xml version="1.0" encoding="utf-8"?>
<!--Prasenjit-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Launch a calculation of selection then view the detailed results--The sorted Objects will be dispalyed in lines, the adequacy and the preselected Attributes will be displayed in columns.in the parenthesis in the 'Displayed Objects' tab, 'number of selected Objects/total number of Objects' will be displayed;There'll be an available adjacent checkbox for each sorted Object;* Double click on a result Object, the Object will be highlighted in the Objects list in TEEXMA interface</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associative test start</Message>
    <Actions>
      <Login>
        <Message>Login</Message>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_MulticriteriaSelection</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
            </Actions>
          </NavigationTree>
          <MultiCriteria>
            <Actions>
              <Content>
                <Actions>
                  <CriteriaTab>
                    <Actions>
                      <Content>
                        <Actions>
                          <LaunchCalculation>
                            <Actions>
                              <Close />
                            </Actions>
                          </LaunchCalculation>
                        </Actions>
                      </Content>
                    </Actions>
                  </CriteriaTab>
                </Actions>
              </Content>
            </Actions>
          </MultiCriteria>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>