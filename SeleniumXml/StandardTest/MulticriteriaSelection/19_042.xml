<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Recursivity --Apply recursivity to criteria--Check the availability of the 'to all' radiobutton;Launch Multicriteria Selection, OT 'TEST_Multicriteriaelection'. Then open the Requirement Set 'RS_Recursivity_all'. Launch calculation of the selection :Only the Objects meeting the limits, the Objects of which the Bidirectional links have target Objects meeting the limits will be listed</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associative test start</Message>
    <Actions>
      <Login>
        <Message>Login</Message>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <MultiCriteria>
            <Actions>
              <Content>
                <Actions>
                  <CriteriaTab>
                    <Actions>
                      <Content>
                        <Actions>
                          <ReqSetObjectType>
                            <Actions>
                              <SelectV2>
                                <Parameters>
                                  <value>TEST_MulticriteriaSelection</value>
                                </Parameters>
                              </SelectV2>
                            </Actions>
                          </ReqSetObjectType>
                          <Sleep>
                            <Parameters>
                              <timeOut>5</timeOut>
                            </Parameters>
                          </Sleep>
                          <DefineCriteria>
                            <Actions>
                              <Content>
                                <Actions>
                                  <Recursivity>
                                    <Parameters>
                                      <linkAttribute>Link_Mul_Direct</linkAttribute>
                                      <applyRecursivity>to all</applyRecursivity>
                                    </Parameters>
                                  </Recursivity>
                                </Actions>
                              </Content>
                              <Close />
                            </Actions>
                          </DefineCriteria>
                        </Actions>
                      </Content>
                    </Actions>
                  </CriteriaTab>
                </Actions>
              </Content>
              <Close />
            </Actions>
          </MultiCriteria>
          <OpenRequirementSet>
            <Actions>
              <Content>
                <Actions>
                  <RequirementSet>
                    <Parameters>
                      <requirementSet>RS_Recursivity_all</requirementSet>
                    </Parameters>
                  </RequirementSet>
                  <Open>
                    <Parameters>
                      <popUp>false</popUp>
                    </Parameters>
                    <Actions>
                      <Content>
                        <Actions>
                          <CriteriaTab>
                            <Actions>
                              <Content>
                                <Actions>
                                  <LaunchCalculation>
                                    <Actions />
                                  </LaunchCalculation>
                                </Actions>
                              </Content>
                            </Actions>
                          </CriteriaTab>
                        </Actions>
                      </Content>
                    </Actions>
                  </Open>
                </Actions>
              </Content>
            </Actions>
          </OpenRequirementSet>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>