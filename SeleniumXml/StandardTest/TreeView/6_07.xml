<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the availability of the dedicated button 'Add Object'.</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_TreeView</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <SwitchReadWrite />
              <AddObject>
                <Parameters />
                <!--no object is highlighted-->
                <Actions>
                  <Content>
                    <Actions>
                      <Write>
                        <DTText>
                          <Parameters>
                            <id>97</id>
                            <value>
                              <value>no object hilighted</value>
                            </value>
                          </Parameters>
                        </DTText>
                      </Write>
                    </Actions>
                  </Content>
                  <Ok>
                    <Parameters>
                      <validate>true</validate>
                    </Parameters>
                  </Ok>
                </Actions>
              </AddObject>
            </Actions>
          </NavigationTree>
          <NavigationTree>
            <Actions>
              <AddObject>
                <Parameters>
                  <nodes>
                    <path>New</path>
                  </nodes>
                </Parameters>
                <Actions>
                  <Content>
                    <Actions>
                      <Write>
                        <DTText>
                          <Parameters>
                            <id>97</id>
                            <value>
                              <value>one object hilighted</value>
                            </value>
                          </Parameters>
                        </DTText>
                      </Write>
                    </Actions>
                  </Content>
                  <Ok>
                    <Parameters>
                      <validate>true</validate>
                    </Parameters>
                  </Ok>
                </Actions>
              </AddObject>
            </Actions>
          </NavigationTree>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>