<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Switch to the 'Same Tab' tab and in the Group 'SameGroup'(For Attributes using the Single Link in this group) and check Decimals(SingleValue, Range, MMM)</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_ReadView_Inheritance</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>T_R_I_1</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <ReadForm>
            <Actions>
              <ChangeTab>
                <Parameters>
                  <tabIndex>3</tabIndex>
                </Parameters>
              </ChangeTab>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>100</attrId>
                  </Parameters>
                  <ExpectedResult>
                    <min>100</min>
                    <unit>
                    	<unitName>µm</unitName>
                    </unit>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>319</attrId>
                  </Parameters>
                  <ExpectedResult>
                    <min>10</min>
                    <max>500</max>
                    <unit>
                      <unitName>µm</unitName>
                    </unit>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>320</attrId>
                  </Parameters>
                  <ExpectedResult>
                    <min>10</min>
                    <max>500</max>
                    <mean>200</mean>
                    <unit>
                      <unitName>µm</unitName>
                    </unit>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>