<?xml version="1.0" encoding="utf-8"?>
<!-- click on the Link 'To Standard Visible Object Import_1' and the Link 'To Standard Invisible Object IV_1'-->
<!--We don't check after reload teexma , OT not display.-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the availability of the link to an Object Standard OT</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <LinkPortal>
            <Parameters>
              <link>To Standard Visible Object 'Import_1'</link>
              
            </Parameters>
          </LinkPortal>
          <ReadBanner>
          	<ExpectedResult>Import_1</ExpectedResult></ReadBanner>
          <NavigationTree>
          	<Actions>
          		<SelectOT>
          			<Actions>
          				<Read>
          					<ExpectedResult>New view101</ExpectedResult></Read>
          				<Select>
          					<Parameters>
          						<value>Portal</value>
          					</Parameters>
          				</Select></Actions>
          		</SelectOT></Actions>
          </NavigationTree>
          <LinkPortal>
          	<Parameters>
          		<link>To Standard Invisible Object 'IV_1'</link>
          	</Parameters>
          </LinkPortal>
          <NavigationTree>
          	<Actions></Actions>
          </NavigationTree>
          <NavigationTree>
          	<Actions>
          		<SelectOT>
          			<Actions>
          				<Read>
          					<ExpectedResult>BV_invisible</ExpectedResult></Read>
          				<Select>
          					<Parameters>
          						<value>TEST_ReadView_Inheritance</value>
          					</Parameters>
          				</Select></Actions>
          		</SelectOT>
          		<RefreshWebPage></RefreshWebPage>
          		<Sleep>
          			<Parameters>
          				<timeOut>2</timeOut>
          			</Parameters>
          		</Sleep>
          		<SelectOT>
          			<Actions>
          				<Select>
          					<Parameters>
          						<value>BV_invisible</value>
          					</Parameters>
          				</Select></Actions>
          		</SelectOT></Actions>
          </NavigationTree>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>