<?xml version="1.0" encoding="utf-8"?>
<!-- click on the Link 'To Standard Visible Object Import_1' and the Link 'To Standard Invisible Object IV_1'-->
<!--We don't check after reload teexma , OT not display.-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the availability of the link to an Object Standard OT</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
        	<ReadNumberOfObjectInRL>
        		<ExpectedResult>1</ExpectedResult></ReadNumberOfObjectInRL>
        	<ExpandNavigationTree></ExpandNavigationTree>
        	<NavigationTree>
        		<Actions>
        			<SelectOT>
        				<Actions>
        					<Select>
        						<Parameters>
        							<value>TEST_Portal/Widget</value>
        						</Parameters>
        					</Select></Actions>
        			</SelectOT>
        			<EntityTree>
        				<Actions>
        					<SelectEntity>
        						<Parameters>
        							<nodes>
        								<path>New</path></nodes>
        						</Parameters>
        					</SelectEntity></Actions>
        			</EntityTree></Actions>
        	</NavigationTree>
        	<Edit>
        		<Actions>
        			<Content>
        				<Actions>
        					<Date>
        						<Parameters>
        							<attrId>95</attrId>
        						</Parameters>
        						<Actions>
        							<Year>
        								<Parameters>
        									<year>2014</year>
        								</Parameters>
        							</Year>
        							<Month>
        								<Parameters>
        									<month>Feb</month>
        								</Parameters>
        							</Month>
        							<Day>
        								<Parameters>
        									<day>4</day>
        								</Parameters>
        							</Day></Actions>
        					</Date>
        					<Write>
        						<DTBool>
        							<Parameters>
        								<id>93</id>
        								<value>
        									<value>true</value>
        								</value>
        							</Parameters>
        						</DTBool>
        					</Write></Actions>
        			</Content>
        			<Ok>
        				<Parameters>
        					<validate>true</validate></Parameters>
        			</Ok></Actions>
        	</Edit>
        	<NavigationTree>
        		<Actions>
        			<SelectOT>
        				<Actions>
        					<Select>
        						<Parameters>
        							<value>Portal</value>
        						</Parameters>
        					</Select></Actions>
        			</SelectOT></Actions>
        	</NavigationTree>
        	<Sleep>
        		<Parameters>
        			<timeOut>3</timeOut>
        		</Parameters>
        	</Sleep>
        	<ReadNumberOfObjectInRL>
        		<ExpectedResult>2</ExpectedResult></ReadNumberOfObjectInRL>

        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>