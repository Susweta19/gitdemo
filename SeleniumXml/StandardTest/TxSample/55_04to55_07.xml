<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Create new "échantillons" from application</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrateur</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>Demandes</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>Request VF</path>
											</nodes>
										</Parameters>
									</SelectEntity>
								</Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<LaunchModel>
						<Parameters>
							<modelName>Manage and route samples</modelName>
						</Parameters>
					</LaunchModel>
					<ReturnManageSample>
						<Actions>
							<Content>
								<Actions>
									<WaitForAjax></WaitForAjax>
									<AddSample>
										<Actions>
											<Content>
												<Actions>
													<WaitForAjax></WaitForAjax>
													<SetValue>
														<Parameters>
															<seriesIndex>1</seriesIndex>
															<colIndex>2</colIndex>
															<value>2</value>
														</Parameters>
													</SetValue>
													<SetValue>
														<Parameters>
															<seriesIndex>1</seriesIndex>
															<colIndex>3</colIndex>
															<value>Hello</value>
														</Parameters>
													</SetValue>
													<SetValue>
														<Parameters>
															<seriesIndex>1</seriesIndex>
															<colIndex>4</colIndex>
															<value>112</value>
														</Parameters>
													</SetValue>
													<SetValue>
														<Parameters>
															<seriesIndex>1</seriesIndex>
															<colIndex>5</colIndex>
															<value>code1</value>
														</Parameters>
													</SetValue>
												</Actions>
											</Content>
											<Ok>
												<Parameters>
													<validate>true</validate>
												</Parameters>
											</Ok>
										</Actions>
									</AddSample>
									<SampleGrid>
										<Actions>
											<WaitForAjax></WaitForAjax><!--Read row 1 -->
											<ReadValue>
												<Parameters>
													<Row>1</Row>
													<Column>2</Column>
												</Parameters>
												<ExpectedResult>-New Sample 1</ExpectedResult>
											</ReadValue>
											<ReadValue>
												<Parameters>
													<Row>1</Row>
													<Column>5</Column>
												</Parameters>
												<ExpectedResult>Hello</ExpectedResult>
											</ReadValue>
											<ReadValue>
												<Parameters>
													<Row>1</Row>
													<Column>6</Column>
												</Parameters>
												<ExpectedResult>112</ExpectedResult>
											</ReadValue>
											<ReadValue>
												<Parameters>
													<Row>1</Row>
													<Column>7</Column>
												</Parameters>
												<ExpectedResult>code1</ExpectedResult>
											</ReadValue><!--read row 2 -->
											<ReadValue>
												<Parameters>
													<Row>2</Row>
													<Column>2</Column>
												</Parameters>
												<ExpectedResult>-New Sample 2</ExpectedResult>
											</ReadValue>
											<ReadValue>
												<Parameters>
													<Row>2</Row>
													<Column>5</Column>
												</Parameters>
												<ExpectedResult>Hello</ExpectedResult>
											</ReadValue>
											<ReadValue>
												<Parameters>
													<Row>2</Row>
													<Column>6</Column>
												</Parameters>
												<ExpectedResult>112</ExpectedResult>
											</ReadValue>
											<ReadValue>
												<Parameters>
													<Row>2</Row>
													<Column>7</Column>
												</Parameters>
												<ExpectedResult>code1</ExpectedResult>
											</ReadValue>
											
										</Actions>
									</SampleGrid></Actions>
							</Content>
						</Actions>
					</ReturnManageSample>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>