<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Export Results</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrateur</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<WaitForAjax></WaitForAjax>
					<MainToolbar>
						<Parameters>
							<modelName>Search samples</modelName>
						</Parameters>
					</MainToolbar>
					<WaitForAjax></WaitForAjax>
					<ReturnTxAdvancedSearch>
						<Actions>
							<Content>
								<Actions>
									<WaitForAjax></WaitForAjax>
									<ReadActiveTab>
										<ExpectedResult>Samples</ExpectedResult></ReadActiveTab>
									<RefreshResult></RefreshResult>
									<ManagePopUp>
										<Actions>
											<CloseConfirmationPopup>
												<Parameters>
													<result>
														true
													</result>
												</Parameters>
											</CloseConfirmationPopup></Actions>
									</ManagePopUp>
									
									<ReturnTableViewResult>
										<Parameters>
											<index>1</index>
										</Parameters>

										<Actions>
											<WaitForAjax></WaitForAjax>
											<MasterCheckBox>
												<Actions>
													<Tick></Tick></Actions>
											</MasterCheckBox></Actions>
									</ReturnTableViewResult>
								</Actions>
							</Content>
						</Actions>
					</ReturnTxAdvancedSearch>
					<Export>
						<Actions></Actions>
					</Export>
					<WaitForAjax></WaitForAjax>
					<Sleep>
						<Parameters>
							<timeOut>15</timeOut>
						</Parameters>
					</Sleep>
					<ManageDownloadedFiles>
						<Actions>
							<GetLastDownloadedFile>
								<Parameters>
									<DownloadDirectory>#DownloadDirectory#</DownloadDirectory>
								</Parameters>
								<Hashtag>#ExportedFile#</Hashtag>
							</GetLastDownloadedFile></Actions>
					</ManageDownloadedFiles>
					<DownloadFileChecking>
						<Parameters>
							<DownloadDirectory>#DownloadDirectory#</DownloadDirectory>
							<fileName>Samples.xlsx</fileName>
							<contains>true</contains>
						</Parameters>
						<ExpectedResult>true</ExpectedResult>
					</DownloadFileChecking>
					<ManageDownloadedFiles>
						<Actions>
							<ReadExcelFile>
								<Parameters>
									<filePath>#ExportedFile#</filePath>
									<sheetName>Data</sheetName>
									<row>4</row>
									<uptoCol>1</uptoCol>
								</Parameters>
								<Actions>
									<ElementAt>
										<Parameters>
											<index>0</index>
										</Parameters>
										<ExpectedResult>New_001</ExpectedResult>
									</ElementAt></Actions>
							</ReadExcelFile>
							<ReadExcelFile>
								<Parameters>
									<filePath>#ExportedFile#</filePath>
									<sheetName>Data</sheetName>
									<row>5</row>
									<uptoCol>1</uptoCol>
								</Parameters>
								<Actions>
									<ElementAt>
										<Parameters>
											<index>0</index>
										</Parameters>
										<ExpectedResult>New_002</ExpectedResult>
									</ElementAt></Actions>
							</ReadExcelFile></Actions>
					</ManageDownloadedFiles>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>