<?xml version="1.0" encoding="utf-8"?>
<!--Right click in the blank areas of the treeview 'Add Object' option-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the availability "Add Object" button</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_BVA_1</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <SwitchReadWrite /><WaitForAjax></WaitForAjax>
            	<AddObject>
            		<Parameters></Parameters>
            		<Actions>
            			<Content>
            				<Actions>
                          <Write>
                            <DTBool>
                              <Parameters>
                                <id>93</id>
                                <value>
                                  <value>
																		true
																	</value>
                                </value>
                              </Parameters>
                            </DTBool>
                          </Write>
                        </Actions>
            			</Content>
            			<Ok>
            				<Parameters>
            					<validate>true</validate></Parameters>
            			</Ok></Actions>
            	</AddObject>
            </Actions>
          </NavigationTree>
          <ReadForm>
            <Actions>
              <Read>
                <DTBool>
                  <Parameters>
                    <attrId>93</attrId>
                  </Parameters>
                  <!--If the object is realy created then the boolean value will be true-->
                  <ExpectedResult>
                    <value>true</value>
                  </ExpectedResult>
                </DTBool>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>