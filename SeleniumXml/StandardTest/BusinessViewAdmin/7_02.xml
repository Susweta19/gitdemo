<?xml version="1.0" encoding="utf-8"?>
<!-- 'Compare the Object to the others' option-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check 'Compare the object' option (Right click on a object) in Readmode</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_BVA_1</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <WaitForAjax></WaitForAjax>
              <TreeContextMenu>
                <Actions>
                  <CompareTheObject>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                    <Actions>
                      <Content>
                        <Actions>
                          <ObjectTree>
                            <Actions>
                              <SelectEntityBox>
                                <Parameters>
                                  <nodes>
                                    <path>Boolean</path>
                                  </nodes>
                                </Parameters>
                                <Actions>
                                  <Read>
                                    <ExpectedResult>
																				true
																			</ExpectedResult>
                                  </Read>
                                </Actions>
                              </SelectEntityBox>
                              <SelectEntityBox>
                                <Parameters>
                                  <nodes>
                                    <path>Date</path>
                                  </nodes>
                                </Parameters>
                                <Actions>
                                  <Read>
                                    <ExpectedResult>
																				true
																			</ExpectedResult>
                                  </Read>
                                </Actions>
                              </SelectEntityBox>
                              <SelectEntityBox>
                                <Parameters>
                                  <nodes>
                                    <path>String</path>
                                  </nodes>
                                </Parameters>
                                <Actions>
                                  <Read>
                                    <ExpectedResult>false</ExpectedResult>
                                  </Read>
                                </Actions>
                              </SelectEntityBox>
                              <DeselectAll />
                              <SelectAll />
                            </Actions>
                          </ObjectTree>
                          <Ok>
                            <Actions>
                              <Close />
                            </Actions>
                          </Ok>
                        </Actions>
                      </Content>
                    </Actions>
                  </CompareTheObject>
                </Actions>
              </TreeContextMenu>
            </Actions>
          </NavigationTree>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>