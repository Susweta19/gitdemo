<?xml version="1.0" encoding="utf-8"?>
<!--Only way to do it-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the availability "Delete the Object" button</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_BVA_1</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <WaitForAjax></WaitForAjax>
              <SwitchReadWrite /><WaitForAjax></WaitForAjax>
            	<TreeContextMenu>
                <Actions>
                  <Duplicate>
                  	<Parameters>
                  		<nodes>
                  			<path>New1</path></nodes>
                  	</Parameters>
                  	<Actions>
                  		<Ok>
                  			<Parameters></Parameters>
                  		</Ok></Actions>
                  </Duplicate>
                </Actions>
              </TreeContextMenu>
            	<WaitForAjax></WaitForAjax>
            	<EntityTree>
            		<Actions>
            			<SelectEntity>
            				<Parameters>
            					<nodes>
            						<path>New</path></nodes>
            				</Parameters>
            			</SelectEntity>
            			<EntitytPresence>
            				<Parameters>
            					<nodes>
            						<path>Copy of New1</path></nodes>
            					<presence>true</presence>
            				</Parameters>
            			</EntitytPresence></Actions>
            	</EntityTree>
            	<DeleteObject>
            		<Parameters>
            			<collection>
            				<collection>
            					<path>Copy of New1</path></collection></collection></Parameters>
            		<Actions>
            			<CloseConfirmationPopup>
            				<Parameters>
            					<result>true</result>
            				</Parameters>
            			</CloseConfirmationPopup></Actions>
            	</DeleteObject>
            	<EntityTree>
            		<Actions>
            			<EntitytPresence>
            				<Parameters>
            					<nodes>
            						<path>Copy of New1</path></nodes>
            					<presence>false</presence>
            				</Parameters>
            			</EntitytPresence></Actions>
            	</EntityTree>
            </Actions>
          </NavigationTree>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>