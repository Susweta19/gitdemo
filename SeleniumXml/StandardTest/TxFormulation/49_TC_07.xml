<?xml version="1.0" encoding="utf-8"?>
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Selection of main ingredient - Selection of ingredient in tree mode</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>Recipe</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>Recipe 1</path>
											</nodes>
										</Parameters>
									</SelectEntity>
								</Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<MainToolbar>
						<Parameters>
							<modelName>Launch formulation</modelName>
						</Parameters>
					</MainToolbar>
					<ReturnIngredientWindow>
						<Actions>
							<ReadHeader>
								<ExpectedResult>Select a main ingredient</ExpectedResult>
							</ReadHeader>
							<IsButtonDisabled>
								<Parameters>
									<buttonName>Ok</buttonName>
								</Parameters>
								<ExpectedResult>true</ExpectedResult>
							</IsButtonDisabled>
							<Content>
								<Actions>
									<SelectIngredient>
										<Actions>
											<SelectEntityBox>
												<Parameters>
													<nodes>
														<path>IAADI0007 - ROUGE K3911 A 1%</path></nodes>
												</Parameters>
												<Actions>
													<Tick></Tick></Actions>
											</SelectEntityBox>
											<IsRadioButtonTicked>
												<Parameters>
													<entityName>IAADI0007 - ROUGE K3911 A 1%</entityName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsRadioButtonTicked></Actions>
									</SelectIngredient></Actions>
							</Content>
							<IsButtonDisabled>
								<Parameters>
									<buttonName>Ok</buttonName>
								</Parameters>
								<ExpectedResult>false</ExpectedResult>
							</IsButtonDisabled>
						</Actions>
					</ReturnIngredientWindow>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>