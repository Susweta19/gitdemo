<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Try to fill in the Attribute 'Month_Answer' with a decimal value then turn back to ReadForm	The value of the Attribute 'Month_Answer' will be the month of the current date</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Utils</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <ChangeTab>
                    <Parameters>
                      <tabIndex>2</tabIndex>
                    </Parameters>
                  </ChangeTab>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>175</id>
                        <!--input for attr "Month_Answer"-->
                        <value>
                          <min>55</min>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <ReadForm>
            <Actions>
              <ChangeTab>
                <Parameters>
                  <tabIndex>2</tabIndex>
                </Parameters>
              </ChangeTab>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>175</attrId>
                  </Parameters>
                  <!--outputforattr"Month_Answer"-->
                  <ExpectedResult>
                    <min>#CurrentMonth#</min>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>