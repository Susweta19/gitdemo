<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Try to fill in the Attributes 'Str_1', 'Str_2' and 'Str_3',  and check the value of the Attribute 'Union_Str_Answer'	The value of the Attribute 'Union_Str_Answer' will be the combination of the 3 entered strings, with '-' as separator</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Utils</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <ChangeTab>
                    <Parameters>
                      <tabIndex>3</tabIndex>
                    </Parameters>
                  </ChangeTab>
                  <Write>
                    <DTText>
                      <Parameters>
                        <id>197</id>
                        <!--input for attr  'Str_1'-->
                        <value>
                          <value>Teexma</value>
                        </value>
                      </Parameters>
                    </DTText>
                  </Write>
                  <Write>
                    <DTText>
                      <Parameters>
                        <id>196</id>
                        <!--input for attr  'Str_2'-->
                        <value>
                          <value>Teexma2</value>
                        </value>
                      </Parameters>
                    </DTText>
                  </Write>
                  <Write>
                    <DTText>
                      <Parameters>
                        <id>195</id>
                        <!--input for attr  'Str_2'-->
                        <value>
                          <value>Teexma3</value>
                        </value>
                      </Parameters>
                    </DTText>
                  </Write>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <ReadForm>
            <Actions>
              <ChangeTab>
                <Parameters>
                  <tabIndex>3</tabIndex>
                </Parameters>
              </ChangeTab>
              <Read>
                <DTText>
                  <Parameters>
                    <attrId>199</attrId>
                  </Parameters>
                  <!--outputforattrUnion_Str_Answer-->
                  <ExpectedResult>
                    <value>Teexma-Teexma2-Teexma3</value>
                  </ExpectedResult>
                </DTText>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>