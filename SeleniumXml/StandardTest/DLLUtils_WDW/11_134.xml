<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Try to fill in the Attribute 'Inc_Alpha' with a string, then turn back to ReadForm and check the value of the Attribute 'Inc_Alpha'	'The value of the Attribute 'Inc_Alpha' will be increased by X (X is the value of the Attribute 'Step')(example : Inc_Alpha = a, Step = 10  ==  Inc_Alpha = K;Inc_Alpha = ab, Step = 100  ==  Inc_Alpha = DX)'</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Utils</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <ChangeTab>
                    <Parameters>
                      <tabIndex>2</tabIndex>
                    </Parameters>
                  </ChangeTab>
                  <Write>
                    <DTText>
                      <Parameters>
                        <id>166</id>
                        <!--input for "Inc_Alpha"-->
                        <value>
                          <value>A</value>
                        </value>
                      </Parameters>
                    </DTText>
                  </Write>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>164</id>
                        <!--input for "Step"-->
                        <value>
                          <min>10</min>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <ReadForm>
            <Actions>
              <ChangeTab>
                <Parameters>
                  <tabIndex>2</tabIndex>
                </Parameters>
              </ChangeTab>
              <Read>
                <DTText>
                  <Parameters>
                    <attrId>166</attrId>
                  </Parameters>
                  <!--outputfor"Inc_Alpha"-->
                  <ExpectedResult>
                    <value>K</value>
                  </ExpectedResult>
                </DTText>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>