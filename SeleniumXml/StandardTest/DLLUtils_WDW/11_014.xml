<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Fill in the 2 textboxes 'String_1' and 'String_2' with any string we want, fill in the Attribute 'Decimal_1' with a Decimal value. Then turn back to ReadForm. Check the value of the Attribute 'Concat_Answer'	"* The value of the Attribute 'Concat_Answer' will be a concatenation of the 3 values and in the following formart :value of String_1--value of String_2--value of Decimal_1</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Utils</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <Write>
                    <DTText>
                      <Parameters>
                        <id>141</id>
                        <!--we write the value in srting1-->
                        <value>
                          <value>abc</value>
                        </value>
                      </Parameters>
                    </DTText>
                  </Write>
                  <Write>
                    <DTText>
                      <Parameters>
                        <id>140</id>
                        <!--we write the value in srting2-->
                        <value>
                          <value>Est</value>
                        </value>
                      </Parameters>
                    </DTText>
                  </Write>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>137</id>
                        <!--we write the value in Decimal1-->
                        <value>
                          <min>1.25</min>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <Sleep>
            <Parameters>
              <timeOut>5</timeOut>
            </Parameters>
          </Sleep>
          <ReadForm>
            <Actions>
              <Read>
                <DTText>
                  <Parameters>
                    <attrId>138</attrId>
                  </Parameters>
                  <!--wereadthevalueafterlaunchingthemodel-->
                  <ExpectedResult>
                    <value>abc--Est--1,25|abc--Est--1.25</value>
                  </ExpectedResult>
                </DTText>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>