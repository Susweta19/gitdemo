<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Fill in the Attributes 'Decimal_1' and 'Decimal_2' then turn back to ReadForm. Check the value of the Attribute 'Div_Answer'	'The value of the Attribute 'Div_Answer' will be the division of the 2 Decimals(Example : if Decimal_1 = 5, Decimal_2 = 3, the Div_Answer will be 1.66667)'</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Utils</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>137</id>
                        <!--we are giving the value in Decimal 1-->
                        <value>
                          <min>50</min>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>136</id>
                        <!--we are giving the value in Decimal 2-->
                        <value>
                          <min>5</min>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <Sleep>
            <Parameters>
              <timeOut>5</timeOut>
            </Parameters>
          </Sleep>
          <ReadForm>
            <Actions>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>148</attrId>
                  </Parameters>
                  <!--afterlaunchingthemodelwereadthevalue-->
                  <ExpectedResult>
                    <min>10</min>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>