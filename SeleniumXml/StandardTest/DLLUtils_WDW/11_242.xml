<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Fill in the Attributes 'Decimal_1', 'Decimal_2' and 'Decimal_3' with numerical values, then turn back to ReadForm and check the values of the Attributes 'Percentage_1', 'Percentage_2' and 'Percentage_3'	'These values will be the percentages of each entered numerical values(example : Decimal_1 = 10, Decimal_2 = 15, Decimal_3 = 20;then Percentage_1 will be 0.222222, Percentage_2 will be 0.333333, Percentage_3 will be 0.444444)'</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Utils</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>137</id>
                        <!--input for attr Decimal_1-->
                        <value>
                          <min>25</min>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>136</id>
                        <!--input for attr Decimal_2-->
                        <value>
                          <min>55</min>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>135</id>
                        <!--input for attr Decimal_3-->
                        <value>
                          <min>65</min>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <Sleep>
            <Parameters>
              <timeOut>3</timeOut>
            </Parameters>
          </Sleep>
          <ReadForm>
            <Actions>
              <ChangeTab>
                <Parameters>
                  <tabIndex>2</tabIndex>
                </Parameters>
              </ChangeTab>
              <Sleep>
                <Parameters>
                  <timeOut>5</timeOut>
                </Parameters>
              </Sleep>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>180</attrId>
                  </Parameters>
                  <!--outputforattr"Percentage_1"-->
                  <ExpectedResult>
                    <min>0,172414|0.172414</min>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>179</attrId>
                  </Parameters>
                  <!--outputforattr"Percentage_2"-->
                  <ExpectedResult>
                    <min>0,37931|0.37931</min>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
              <Read>
                <DTDecimalValue>
                  <Parameters>
                    <attrId>178</attrId>
                  </Parameters>
                  <!--outputforattr"Percentage_3"-->
                  <ExpectedResult>
                    <min>0,448276|0.448276</min>
                  </ExpectedResult>
                </DTDecimalValue>
              </Read>
            </Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>