<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Generate a curve with a BlackBox Template (Draw a regression curve)</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<CurveModule>
						<Actions>
							<Content>
								<Actions>
									<AddNewCurve>
										<Actions>
											<Content>
												<Actions>
													<SourceType>
														<Parameters>
															<typeName>Black Box Template</typeName>
														</Parameters>
													</SourceType>
													<Next></Next>
													<Finish></Finish></Actions>
											</Content></Actions>
									</AddNewCurve>
									<Regression>
										<Actions>
											<Content>
												<Actions>
													<RegressionType>
														<Actions>
															<Select>
																<Parameters>
																	<value>Linear</value>
																</Parameters>
															</Select></Actions>
													</RegressionType>
													<Calculate></Calculate>
													<Equation>
														<Actions>
															<Read>
																<ExpectedResult>y = 2.988e-1x + -1.793e-1</ExpectedResult></Read></Actions>
													</Equation>
													<AddRegression></AddRegression></Actions>
											</Content></Actions>
									</Regression>
									<Regression>
										<Actions>
											<Content>
												<Actions>
													<SelectCurve>
														<Actions>
															<Select>
																<Parameters>
																	<value>Curve&#160;B</value>
																</Parameters>
															</Select></Actions>
													</SelectCurve>
													<RegressionType>
														<Actions>
															<Select>
																<Parameters>
																	<value>Exponential</value>
																</Parameters>
															</Select></Actions>
													</RegressionType>
													<Calculate></Calculate>
													<Equation>
														<Actions>
															<Read>
																<ExpectedResult>y = 1.000e+0e^(1.000e+0x)</ExpectedResult></Read>
														</Actions>
													</Equation></Actions>
											</Content></Actions>
									</Regression></Actions>
							</Content>
						</Actions>
					</CurveModule>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>