<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Generate a curve with a BlackBox Template (Modify Curve B)</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<CurveModule>
						<Actions>
							<Content>
								<Actions>
									<AddNewCurve>
										<Actions>
											<Content>
												<Actions>
													<SourceType>
														<Parameters>
															<typeName>Black Box Template</typeName>
														</Parameters>
													</SourceType>
													<AbscissasLowerbound>
														<Actions>
															<Write>
																<Parameters>
																	<value>-5</value>
																</Parameters>
															</Write></Actions>
													</AbscissasLowerbound>
													<AbscissasUpperbound>
														<Actions>
															<Write>
																<Parameters>
																	<value>5</value>
																</Parameters>
															</Write></Actions>
													</AbscissasUpperbound>
													<EditNumberOfPoint>
														<Actions>
															<Write>
																<Parameters>
																	<value>100</value>
																</Parameters>
															</Write></Actions>
													</EditNumberOfPoint>
													<Next></Next>
													<TabChartType>
														<Actions>
															<ChangeCurve>
																<Parameters>
																	<tabName>Curve B</tabName>
																</Parameters>
															</ChangeCurve>
															<ManageSeriesTable>
																<Actions>
																	<EditCellValue>
																		<Parameters>
																			<row>1</row>
																			<col>2</col>
																		</Parameters>
																		<Actions>
																			<Write>
																				<Parameters>
																					<value>8</value>
																				</Parameters>
																			</Write>
																			<PressEnter></PressEnter></Actions>
																	</EditCellValue></Actions>
															</ManageSeriesTable>
															<ManageAppearance>
																<Actions>
																	<SelectPointsStyle>
																		<Parameters>
																			<style>1</style>
																		</Parameters>
																	</SelectPointsStyle>
																	<SelectCurveColor>
																		<Actions>
																			<SelectColor>
																				<Parameters>
																					<index>4</index>
																					<chartType>scatter</chartType>
																				</Parameters>
																			</SelectColor></Actions>
																	</SelectCurveColor></Actions>
															</ManageAppearance>
														</Actions>
													</TabChartType>
													<Finish></Finish></Actions>
											</Content></Actions>
									</AddNewCurve></Actions>
							</Content>
						</Actions>
					</CurveModule>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>