<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Generate a curve with a BlackBox Template (Export the chart)</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<CurveModule>
						<Actions>
							<Content>
								<Actions>
									<AddNewCurve>
										<Actions>
											<Content>
												<Actions>
													<SourceType>
														<Parameters>
															<typeName>Black Box Template</typeName>
														</Parameters>
													</SourceType>
													<Next>
													</Next>
													<Finish>
													</Finish>
												</Actions>
											</Content>
										</Actions>
									</AddNewCurve>
									<ExportChart>
										<Actions>
											<Content>
												<Actions>
													<Format>
														<Actions>
															<Select>
																<Parameters>
																	<value>File</value>
																</Parameters>
															</Select>
														</Actions>
													</Format>
													<SetHeaderLinesToSkip>
														<Actions>
															<Write>
																<Parameters>
																	<value>4</value>
																</Parameters>
															</Write>
														</Actions>
													</SetHeaderLinesToSkip>
													<Separator>
														<Actions>
															<Select>
																<Parameters>
																	<value>,</value>
																</Parameters>
															</Select>
														</Actions>
													</Separator>
													<FileName>
														<Actions>
															<Write>
																<Parameters>
																	<value>.csv</value>
																</Parameters>
															</Write>
														</Actions>
													</FileName>
												</Actions>
											</Content>
											<Ok>
												<Parameters></Parameters>
											</Ok>
											
										</Actions>
									</ExportChart>
								</Actions>
							</Content>
						</Actions>
					</CurveModule>
					<DownloadFileChecking>
						<Parameters>
							<DownloadDirectory>#DownloadDirectory#</DownloadDirectory>
							<fileName>Chart Name.csv</fileName>
						</Parameters>
					</DownloadFileChecking>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>