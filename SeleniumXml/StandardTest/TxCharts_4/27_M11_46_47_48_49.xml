<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Change the "advanced" settings  of curve and reset the curve.</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<CurveModule>
						<Actions>
							<Content>
								<Actions>
									<AddNewCurve>
										<Actions>
											<Content>
												<Actions>
													<SourceType>
														<Parameters>
															<typeName>Local file</typeName>
														</Parameters>
													</SourceType>
													<ChooseFile>
														<Parameters>
															<pathFile>#DocPath#Graph.csv</pathFile>
														</Parameters>
													</ChooseFile>
													<Sleep>
														<Parameters>
															<timeOut>5</timeOut>
														</Parameters>
													</Sleep>
													<Next>
													</Next>
													<Sleep>
														<Parameters>
															<timeOut>3</timeOut>
														</Parameters>
													</Sleep>
													<TabChartType>
														<Actions>
															<IsButtonDisabled>
																<Parameters>
																	<buttonName>Previous</buttonName>
																</Parameters>
																<ExpectedResult>false</ExpectedResult>
															</IsButtonDisabled>
															<IsButtonDisabled>
																<Parameters>
																	<buttonName>Finish</buttonName>
																</Parameters>
																<ExpectedResult>false</ExpectedResult>
															</IsButtonDisabled>
															<IsButtonDisabled>
																<Parameters>
																	<buttonName>Cancel</buttonName>
																</Parameters>
																<ExpectedResult>false</ExpectedResult>
															</IsButtonDisabled>
															<IsAdvanceApprencePresent>
																<Parameters>
																	<Name>Advanced</Name>
																</Parameters>
																<ExpectedResult>true</ExpectedResult>
															</IsAdvanceApprencePresent>
															<IsAdvanceApprencePresent>
																<Parameters>
																	<Name>Appearance</Name>
																</Parameters>
																<ExpectedResult>true</ExpectedResult>
															</IsAdvanceApprencePresent>
														</Actions>
													</TabChartType>
													<Finish>
													</Finish>
												</Actions>
											</Content>
										</Actions>
									</AddNewCurve>
									<NewChart>
										<Actions>
											<ReadPopUpMessage>
												
												<Actions>
													<ElementAt>
														<Parameters>
															<index>0</index>
														</Parameters>
														<ExpectedResult>Do you want to reset the workspace?</ExpectedResult>
													</ElementAt>
												</Actions>
											</ReadPopUpMessage>
										</Actions>
									</NewChart>
								</Actions>
							</Content>
						</Actions>
					</CurveModule>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>