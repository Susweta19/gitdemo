<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the ability of Uploading file with Special Character in document file in write mode in "Source"  with document visualization enable</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_WriteView</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters><nodes><path>AllDataPresentToInherit</path></nodes></Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
              	<Actions>
              		<Source>
              			<Parameters>
              				<attrId>93</attrId>
              			</Parameters>
              			<Actions>
              				<Content>
              					<Actions>
              						<ChangeTab>
              							<Parameters>
              								<tabIndex>2</tabIndex>
              							</Parameters>
              						</ChangeTab>
              						<DeleteAllUploadedFile>
              							<Parameters>
              								<attrId>149</attrId>
              							</Parameters>
              						</DeleteAllUploadedFile></Actions>
              				</Content></Actions>
              		</Source></Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
              <Ok>
              	<Parameters>
              		<validate>true</validate></Parameters>
              </Ok>
            </Actions>
          </Edit>
          <Edit>
            <Actions>
              <Content>
              	<Actions>
              		<Source>
              			<Parameters>
              				<attrId>93</attrId>
              			</Parameters>
              			<Actions>
              				<Content>
              					<Actions>
              						<ChangeTab>
              							<Parameters>
              								<tabIndex>2</tabIndex>
              							</Parameters>
              						</ChangeTab>
              						<PublishDocumentFile>
              							<Parameters>
              								<attrId>149</attrId>
              								<pathFile>#DocPath#PDF\%.pdf</pathFile>
              							</Parameters>
              						</PublishDocumentFile>
              						<EnableDocumentVisualization>
              							<Parameters>
              								<attrId>149</attrId>
              							</Parameters>
              						</EnableDocumentVisualization></Actions>
              				</Content></Actions>
              		</Source></Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
              <Ok>
              	<Parameters>
              		<validate>true</validate></Parameters>
              </Ok>
            </Actions>
          </Edit>
          <ReadForm>
          	<Actions>
          		<Source>
          			<Parameters>
          				<attrId>93</attrId>
          			</Parameters>
          			<Actions>
          				<Content>
          					<Actions>
          						<ChangeTab>
          							<Parameters>
          								<tabIndex>2</tabIndex>
          							</Parameters>
          						</ChangeTab>
          						<ManagePreviewFile>
          							<Parameters>
          								<attrId>149</attrId>
          							</Parameters>
          							<Actions>
          								<IsDocumentViewerPresent>
          									<Parameters>
          										<fileName>%(1).pdf</fileName>
          									</Parameters>
          									<ExpectedResult>
          										true
          									</ExpectedResult>
          								</IsDocumentViewerPresent></Actions>
          						</ManagePreviewFile></Actions>
          				</Content></Actions>
          		</Source></Actions>
          </ReadForm>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>