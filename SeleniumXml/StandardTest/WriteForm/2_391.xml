<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the functionality  of Decimal Advanced (Standard View) in writeform</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_WriteView</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>New</path>
											</nodes>
										</Parameters>
									</SelectEntity>
								</Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<Edit>
						<Actions>
							<Content>
								<Actions>
									<ChangeTab>
										<Parameters>
											<tabIndex>1</tabIndex>
										</Parameters>
									</ChangeTab>
									<Write>
										<DTDecimalValue>
											<Parameters>
												<id>103</id>
												<value>
													<min>String</min>
												</value>
											</Parameters>
										</DTDecimalValue>

									</Write>
									<Write>
										<DTDecimalValue>
											<Parameters>
												<id>103</id>
												<value>
													<min>123456789</min>
												</value>
											</Parameters>
										</DTDecimalValue>

									</Write>
								</Actions>
							</Content>
							<Ok>
								<Parameters>
									<validate>true</validate>
								</Parameters>
							</Ok>
						</Actions>
					</Edit>
					<ReadForm>
						<Actions>
							<ChangeTab>
								<Parameters>
									<tabIndex>1</tabIndex>
								</Parameters>
							</ChangeTab>
							<Read>
								<DTDecimalValue>
									<Parameters>
										<attrId>103</attrId>
									</Parameters>
									<ExpectedResult>
										<min>1,235e+8|1.235e+8</min>
									</ExpectedResult>
								</DTDecimalValue>

							</Read>
						</Actions>
					</ReadForm>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>