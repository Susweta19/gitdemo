<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the functionality mode of 'Previous' and the 'Next' buttons in 'Date' Attribute .</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_WriteView</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>New</path>
											</nodes>
										</Parameters>
									</SelectEntity>
								</Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<Edit>
						<Actions>
							<Content>
								<Actions>
									<ManageCommonMethods>
										<Actions>
											<GetNextMonthName>
												<Parameters>
													<monthLength>-1</monthLength>
												</Parameters>
												<Hashtag>#NextMonth#</Hashtag>
											</GetNextMonthName>
											<CurrentMonth>
												<Parameters>
													<monthLength>-1</monthLength>
												</Parameters>
												<Hashtag>#PrevMonth#</Hashtag>
											</CurrentMonth>
										</Actions>
									</ManageCommonMethods>
									<DeleteData>
										<DTDate>
											<Parameters>
												<id>95</id>
											</Parameters>
										</DTDate>

									</DeleteData>
									<Date>
										<Parameters>
											<attrId>95</attrId>
										</Parameters>
										<Actions>
											<Next></Next>
											<ReadDisplayedMonth>
												<ExpectedResult>#NextMonth#</ExpectedResult>
											</ReadDisplayedMonth>
											<Prev></Prev>
											<ReadDisplayedMonth>
												<ExpectedResult>#PrevMonth#</ExpectedResult>
											</ReadDisplayedMonth>
										</Actions>
									</Date>
								</Actions>
							</Content>
							<Cancel>
							</Cancel>

						</Actions>
					</Edit>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>