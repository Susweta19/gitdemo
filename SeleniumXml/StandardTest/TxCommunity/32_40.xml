<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the availibility of the "AddANewThread" button</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
					<langage>Spanish</langage>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<Sleep>
						<Parameters>
							<timeOut>5</timeOut>
						</Parameters>
					</Sleep>
					<ExpandCollapseTxCommunity></ExpandCollapseTxCommunity>
					<TxCommunity>
						<Actions>
							<AddNewThread>
								<Actions>
									<Sleep>
										<Parameters>
											<timeOut>3</timeOut>
										</Parameters>
									</Sleep>
									<IsAttributePresent>
										<Parameters>
											<attributeName>Qu'en pensez-vous ?</attributeName>
										</Parameters>
									</IsAttributePresent>
									<IsAttributePresent>
										<Parameters>
											<attributeName>Qui alerter ?</attributeName>
										</Parameters>
										<ExpectedResult>true</ExpectedResult>
									</IsAttributePresent>
									<IsAttributePresent>
										<Parameters>
											<attributeName>Confidentialité :</attributeName>
										</Parameters>
										<ExpectedResult>true</ExpectedResult>
									</IsAttributePresent>
									<IsAttributePresent>
										<Parameters>
											<attributeName>Publique</attributeName>
										</Parameters>
										<ExpectedResult>true</ExpectedResult>
									</IsAttributePresent>
									<IsAttributePresent>
										<Parameters>
											<attributeName>Privée</attributeName>
										</Parameters>
										<ExpectedResult>true</ExpectedResult>
									</IsAttributePresent>
									<ReadHeader>
										<ExpectedResult>Ajouter un nouveau fil de discussion</ExpectedResult>
									</ReadHeader>
								</Actions>
							</AddNewThread>
						</Actions>
					</TxCommunity>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>