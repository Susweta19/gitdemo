<?xml version="1.0" encoding="utf-8"?>
<!--Shubhajit -->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check 'View the entire tree' in "People to notify" link and "Groups to notify" link</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<Sleep>
						<Parameters>
							<timeOut>5</timeOut>
						</Parameters>
					</Sleep>
					<ExpandCollapseTxCommunity></ExpandCollapseTxCommunity>
					<TxCommunity>
						<Actions>
							<AddNewThread>
								<Actions>
									<WhatsOnYourMind>
										<Actions>
											<Write>
												<Parameters>
													<value>Teexma</value>
												</Parameters>
											</Write>
										</Actions>
									</WhatsOnYourMind>
									<Privacy>
										<Parameters>
											<property>true</property>
										</Parameters>
									</Privacy>
									<PeopleToNotify>
										<Actions>
											<SelectEntitiesBox>
												<Parameters>
													<collection>
														<collection>
															<path>New</path>
														</collection>
														<collection>
															<path>New1</path>
														</collection>
													</collection>
												</Parameters>
												<Actions>
													<Tick />
												</Actions>
											</SelectEntitiesBox>
											<ViewCheckedObjects />
											<DisplayTheEntireTree />
										</Actions>
									</PeopleToNotify>
									<Privacy>
										<Parameters>
											<property>false</property>
										</Parameters>
									</Privacy>
									<GroupsToNotify>
										<Actions>
											<SelectEntitiesBox>
												<Parameters>
													<collection>
														<collection>
															<path>Community2</path>
														</collection>
														<collection>
															<path>Community3</path>
														</collection>
													</collection>
												</Parameters>
												<Actions>
													<Tick />
												</Actions>
											</SelectEntitiesBox>
											<ViewCheckedObjects />
											<DisplayTheEntireTree />
										</Actions>
									</GroupsToNotify>
								</Actions>
							</AddNewThread>
						</Actions>
					</TxCommunity>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>