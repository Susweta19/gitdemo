<?xml version="1.0" encoding="utf-8"?>
<!--Check the ability to open the 'Export' window-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the availability of the 'Filter' dropdown list</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Export</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
            </Actions>
          </NavigationTree>
          <Export>
            <Actions>
              <Content>
                <Actions>
                  <Filtre>
                    <Actions>
                      <Read>
                        <ExpectedResult>TEST_Export</ExpectedResult>
                      </Read>
                      <Select>
                      	<Parameters>
                      		<value>TEST_Export_Inheritance</value>
                      	</Parameters>
                      </Select>
                      <Read>
                      	<ExpectedResult>TEST_Export_Inheritance</ExpectedResult></Read>
                      <Select>
                      	<Parameters>
                      		<value>People</value>
                      	</Parameters>
                      </Select>
                      <Read>
                      	<ExpectedResult>People</ExpectedResult></Read>
                      <Select>
                      	<Parameters>
                      		<value>Project</value>
                      	</Parameters>
                      </Select>
                      <Read>
                      	<ExpectedResult>Project</ExpectedResult></Read>
                      <Select>
                      	<Parameters>
                      		<value>Folder</value>
                      	</Parameters>
                      </Select>
                    </Actions>
                  </Filtre>
                </Actions>
              </Content>
              <Close></Close>
            </Actions>
          </Export>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>