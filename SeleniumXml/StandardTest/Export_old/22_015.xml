<?xml version="1.0" encoding="utf-8"?>
<!--Check the ability to open the 'Export' window-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check The Functionlity of UnCheck All in DataList</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Export</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
            </Actions>
          </NavigationTree>
          <Export>
            <Actions>
              <Content>
                <Actions>
                	<DataTree>
                		<Actions>                			
                			<SelectAll></SelectAll>
                			<SelectEntityBox>
                				<Parameters>
                					<nodes>
                						<path>Boolean</path></nodes>
                				</Parameters>
                				<Actions>
                					<Read>
                						<ExpectedResult>true</ExpectedResult></Read></Actions>
                			</SelectEntityBox>
                			<SelectEntityBox>
                				<Parameters>
                					<nodes>
                						<path>Single Associative</path></nodes>
                				</Parameters>
                				<Actions>
                					<Read>
                						<ExpectedResult>true</ExpectedResult></Read></Actions>
                			</SelectEntityBox>
                			<UncheckAll></UncheckAll>  
                			<SelectEntityBox>
                				<Parameters>
                					<nodes>
                						<path>Boolean</path></nodes>
                				</Parameters>
                				<Actions>
                					<Read>
                						<ExpectedResult>false</ExpectedResult></Read></Actions>
                			</SelectEntityBox>
                			<SelectEntityBox>
                				<Parameters>
                					<nodes>
                						<path>Single Associative</path></nodes>
                				</Parameters>
                				<Actions>
                					<Read>
                						<ExpectedResult>false</ExpectedResult></Read></Actions>
                			</SelectEntityBox>
                			<SelectAll></SelectAll>
                			<SelectEntity>
                				<Parameters>
                					<nodes>
                						<path>Decimal</path></nodes>
                				</Parameters>
                			</SelectEntity>
                			<UncheckAll></UncheckAll>
                			<SelectEntityBox>
                				<Parameters>
                					<nodes>
                						<path>String</path></nodes>
                				</Parameters>
                				<Actions>
                					<Read>
                						<ExpectedResult>true</ExpectedResult></Read></Actions>
                			</SelectEntityBox>
                			<SelectEntityBox>
                				<Parameters>
                					<nodes>
                						<path>Dec_SingleValue_NoUnit</path></nodes>
                				</Parameters>
                				<Actions>
                					<Read>
                						<ExpectedResult>false</ExpectedResult></Read></Actions>
                			</SelectEntityBox>
                			<SelectEntityBox>
                				<Parameters>
                					<nodes>
                						<path>Dec_Range_WithBounds</path></nodes>
                				</Parameters>
                				<Actions>
                					<Read>
                						<ExpectedResult>false</ExpectedResult></Read></Actions>
                			</SelectEntityBox>
                			<SelectAll></SelectAll>
                			
                			<SelectEntity>
                				<Parameters>
                					<nodes>
                						<path>Dec_Range_WithBounds</path></nodes>
                				</Parameters>
                			</SelectEntity>
                			<UncheckAll></UncheckAll>
                			
                			<SelectEntityBox>
                				<Parameters>
                					<nodes>
                						<path>Dec_Range_WithBounds</path></nodes>
                				</Parameters>
                				<Actions>
                					<Read>
                						<ExpectedResult>false</ExpectedResult></Read></Actions>
                			</SelectEntityBox>
                			<SelectEntityBox>
                				<Parameters>
                					<nodes>
                						<path>Dec_SingleValue_NoUnit</path></nodes>
                				</Parameters>
                				<Actions>
                					<Read>
                						<ExpectedResult>true</ExpectedResult></Read></Actions>
                			</SelectEntityBox>
                		</Actions>
                	</DataTree>
                </Actions>
              </Content>
              <Close></Close>
            </Actions>
          </Export>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>