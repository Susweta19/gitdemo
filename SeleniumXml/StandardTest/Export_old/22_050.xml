<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the Attribute data placement for ExcelTemplate file File List</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_Export</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>New</path>
											</nodes>
										</Parameters>
									</SelectEntity>
								</Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<Edit>
						<Actions>
							<Content>
								<Actions>
									<ChangeTab>
										<Parameters>
											<tabIndex>2</tabIndex>
										</Parameters>
									</ChangeTab>
									<DeleteAllUploadedFile>
										<Parameters>
											<attrId>124</attrId>
										</Parameters>
									</DeleteAllUploadedFile>
									<PublishDocumentFile>
										<Parameters>
											<attrId>124</attrId>
											<pathFile>#DocPath#Lafarge.bmp</pathFile>
										</Parameters>
									</PublishDocumentFile>
									<WaitForAjax></WaitForAjax>
									<DisableDocumentVisualization>
										<Parameters>
											<attrId>124</attrId>
										</Parameters>
									</DisableDocumentVisualization>
									<PublishDocumentFile>
										<Parameters>
											<attrId>124</attrId>
											<pathFile>#DocPath#DocToLoad.txt</pathFile>
										</Parameters>
									</PublishDocumentFile>
									<WaitForAjax></WaitForAjax>
									<DisableDocumentVisualization>
										<Parameters>
											<attrId>124</attrId>
										</Parameters>
									</DisableDocumentVisualization>
								</Actions>
							</Content>
							<Ok>
								<Parameters>
									<validate>true</validate>
								</Parameters>
							</Ok>
						</Actions>
					</Edit>
					<Export>
						<Actions>
							<Content>
								<Actions>
									<Exports>
										<Actions>
											<Select>
												<Parameters>
													<value>[Excel]&#160;Standard&#160;Export</value>
												</Parameters>
											</Select>
										</Actions>
									</Exports>
									<WaitForAjax></WaitForAjax>
									<DataTree>
										<Actions>
											<UncheckAll></UncheckAll>
											<SelectEntitiesBox>
												<Parameters>
													<collection>
														<collection>
															<path>Documents</path>
															<path>{Document_File}</path>
														</collection>
													</collection>
												</Parameters>
												<Actions>
													<Tick></Tick>
												</Actions>
											</SelectEntitiesBox>

										</Actions>
									</DataTree>
									<Export></Export>
								</Actions>
							</Content>
						</Actions>
					</Export>
					<Sleep>
						<Parameters>
							<timeOut>5</timeOut>
						</Parameters>
					</Sleep>
					<ManageDownloadedFiles>
						<Actions>
							<GetLastDownloadedFile>
								<Parameters>
									<DownloadDirectory>#DownloadDirectory#</DownloadDirectory>
								</Parameters>
								<Hashtag>#downloadDir#</Hashtag>
							</GetLastDownloadedFile>
							<ReadExcelFile>
								<Parameters>
									<filePath>#downloadDir#</filePath>
									<sheetName>TEST_Export</sheetName>
									<row>12</row>
									<uptoCol>2</uptoCol>
								</Parameters>
								<Actions>
									<ElementAt>
										<Parameters>
											<index>0</index>
										</Parameters>
										<ExpectedResult>New</ExpectedResult>
									</ElementAt>
									<ElementAt>
										<Parameters>
											<index>1</index>
										</Parameters><!--Lafarge(1).bmp &#xA; DocToLoad.txt-->
										<ExpectedResult>Lafarge(1).bmp | DocToLoad.txt</ExpectedResult>
									</ElementAt>
								</Actions>
							</ReadExcelFile>
						</Actions>
					</ManageDownloadedFiles>

				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>