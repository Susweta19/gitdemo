<?xml version="1.0" encoding="utf-8"?>
<!--Check the ability to open the 'Export' window-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check The Functionlity of Display the entire tree in DataList</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Export</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
            </Actions>
          </NavigationTree>
          <Export>
            <Actions>
              <Content>
                <Actions>
                	<DataTree>
                		<Actions>
                			<UncheckAll></UncheckAll>
                			<SelectEntityBox>
                				<Parameters>
                					<nodes>
                						<path>Boolean</path></nodes>
                				</Parameters>
                				<Actions>
                					<Tick></Tick></Actions>
                			</SelectEntityBox>
                			<ViewCheckedObjects></ViewCheckedObjects>
                			<EntitytPresence>
                				<Parameters>
                					<nodes>
                						<path>Boolean</path></nodes>
                					<presence>true</presence>
                				</Parameters>
                			</EntitytPresence>
                			<EntitytPresence>
                				<Parameters>
                					<nodes>
                						<path>Single Associative</path></nodes>
                					<presence>false</presence>
                				</Parameters>
                			</EntitytPresence>
                			<DisplayTheEntireTree></DisplayTheEntireTree>
                			<EntitytPresence>
                				<Parameters>
                					<nodes>
                						<path>Boolean</path></nodes>
                					<presence>true</presence>
                				</Parameters>
                			</EntitytPresence>
                			<EntitytPresence>
                				<Parameters>
                					<nodes>
                						<path>Single Associative</path></nodes>
                					<presence>true</presence>
                				</Parameters>
                			</EntitytPresence></Actions>
                	</DataTree>
                </Actions>
              </Content>
              <Close></Close>
            </Actions>
          </Export>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>