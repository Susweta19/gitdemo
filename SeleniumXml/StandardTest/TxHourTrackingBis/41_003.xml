<?xml version="1.0" encoding="utf-8"?>
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the view of Add task(s) window</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Alice&#160;Dupont</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<LaunchModel>
						<Parameters>
							<modelName>Hour tracking in window mode without TxTableView</modelName>
						</Parameters>
					</LaunchModel>
					<ReturnTxHourTrackingInWindow>
						<Actions>
							<WaitForAjax></WaitForAjax>
							<Sleep>
								<Parameters>
									<timeOut>2</timeOut>
								</Parameters>
							</Sleep>
							<ReadHeader>
								<ExpectedResult>Hour Tracking - Spent times</ExpectedResult>
							</ReadHeader>
							<Content>
								<Actions>
									<ManageTask>
										<Actions>
											<DeleteAllPresentTask></DeleteAllPresentTask>
											<AddTask>
												<Parameters>
													<user>Alice Dupont</user>
												</Parameters>
												<Actions>
													<ReadHeader>
														<ExpectedResult>Add task(s)</ExpectedResult>
													</ReadHeader>
													<Content>
														<Actions>
															<!-- We are adding task first just to check weather selected 
																task remains disabled in the Task Selection window -->
															<SelectTask>
																<Actions>
																	<Search>
																		<Parameters>
																			<research>re</research>
																		</Parameters>
																	</Search>
																	<SelectEntitiesBox>
																		<Parameters>
																			<collection>
																				<collection>
																					<path>Réunion</path>
																				</collection>
																			</collection>
																		</Parameters>
																		<Actions>
																			<Tick></Tick>
																		</Actions>
																	</SelectEntitiesBox>
																</Actions>
															</SelectTask>
															<SelectDate>
																<Actions>
																	<Today></Today></Actions>
															</SelectDate>
															<WriteDuration>
																<Actions>
																	<Write>
																		<Parameters>
																			<value>5</value>
																		</Parameters>
																	</Write></Actions>
															</WriteDuration>															
														</Actions>
													</Content>
													<Ok>
														<Parameters>
															<validate>true</validate>
														</Parameters>
													</Ok>
												</Actions>
											</AddTask>
											<PressKeyFromKeyBoard>
												<Actions>
													<PressEnter></PressEnter></Actions>
											</PressKeyFromKeyBoard>
											<IsTaskPresent>
												<Parameters>
													<taskName>Réunion</taskName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsTaskPresent>
											<AddTask>
												<Parameters>
													<user>Alice Dupont</user>
												</Parameters>
												<Actions>
													<Content>
														<Actions>
															<Sleep>
																<Parameters>
																	<timeOut>3</timeOut>
																</Parameters>
															</Sleep>
															<Search>
																<Parameters>
																	<research>re</research>
																</Parameters>
															</Search><!--Checking weather the task remains disabled or 
																not -->
															<IsTaskDisabled>
																<Parameters>
																	<taskName>Réunion</taskName>
																</Parameters>
																<ExpectedResult>true</ExpectedResult>
															</IsTaskDisabled>
															<WaitForAjax></WaitForAjax>
															<IsButtonDisabled>
																<Parameters>
																	<buttonName>Ok</buttonName>
																</Parameters>
																<ExpectedResult>true</ExpectedResult>
															</IsButtonDisabled>

															<IsDateFillDisabled>
																<ExpectedResult>true</ExpectedResult>
															</IsDateFillDisabled>
															<IsDurationDisabled>
																<ExpectedResult>true</ExpectedResult>
															</IsDurationDisabled>
															<IsCommentSectionDisabled>
																<ExpectedResult>true</ExpectedResult>
															</IsCommentSectionDisabled>
															<SelectTask>
																<Actions>
																	<Search>
																		<Parameters>
																			<research>910</research>
																		</Parameters>
																	</Search>
																	<SelectEntitiesBox>
																		<Parameters>
																			<collection>
																				<collection>
																					<path>Task_000910</path>
																				</collection>
																			</collection>
																		</Parameters>
																		<Actions>
																			<Tick></Tick>
																		</Actions>
																	</SelectEntitiesBox>
																</Actions>
															</SelectTask>
															<WaitForAjax></WaitForAjax>
															<IsButtonDisabled>
																<Parameters>
																	<buttonName>Ok</buttonName>
																</Parameters>
																<ExpectedResult>false</ExpectedResult>
															</IsButtonDisabled>

															<IsDateFillDisabled>
																<ExpectedResult>false</ExpectedResult>
															</IsDateFillDisabled>
															<IsDurationDisabled>
																<ExpectedResult>true</ExpectedResult>
															</IsDurationDisabled>
															<IsCommentSectionDisabled>
																<ExpectedResult>true</ExpectedResult>
															</IsCommentSectionDisabled>
															<SelectDate>
																<Actions>
																	<Today></Today>
																</Actions>
															</SelectDate>
															<WaitForAjax></WaitForAjax>
															<IsDurationDisabled>
																<ExpectedResult>false</ExpectedResult>
															</IsDurationDisabled>
															<IsCommentSectionDisabled>
																<ExpectedResult>false</ExpectedResult>
															</IsCommentSectionDisabled>
															<!-- Not Checking the default button for windows i.e Park, 
																Min/Max and Close -->
														</Actions>
													</Content>
													<Ok>
														<Parameters>
															<validate>false</validate>
														</Parameters>
													</Ok>
												</Actions>
											</AddTask>
										</Actions>
									</ManageTask>
								</Actions>
							</Content>
						</Actions>
					</ReturnTxHourTrackingInWindow>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>