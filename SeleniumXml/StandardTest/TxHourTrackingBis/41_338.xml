<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the ability to add multiple tasks for user under My Task</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Alice&#160;Dupont</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<LaunchModel>
						<Parameters>
							<modelName>Hour tracking in new tab  mode with TxTableView</modelName>
						</Parameters>
					</LaunchModel>
					<ReturnTxHourTrackingInNewTab>
						<Actions>
							<Content>
								<Actions>
									<ManageTask>
										<Actions>
											<DeleteAllPresentTask></DeleteAllPresentTask>
											<AddTask>
												<Parameters>
													<user>Alice Dupont</user>
												</Parameters>
												<Actions>
													<ReadHeader>
														<ExpectedResult>Add task(s)</ExpectedResult>
													</ReadHeader>
													<Content>
														<Actions>
															<!-- We are adding task first just to check weather selected 
																task remains disabled in the Task Selection window -->
															<SelectTaskV2>
																<Actions>
																	<CheckBox>
																		<Parameters>
																			<Row>3</Row>
																			<Column>1</Column>
																		</Parameters>
																		<Actions>
																			<Tick></Tick>
																		</Actions>
																	</CheckBox>
																	<CheckBox>
																		<Parameters>
																			<Row>1</Row>
																			<Column>1</Column>
																		</Parameters>
																		<Actions>
																			<Tick></Tick>
																		</Actions>
																	</CheckBox>
																	<ReadValue>
																		<Parameters>
																			<Row>3</Row>
																			<Column>1</Column>
																		</Parameters>
																		<Hashtag>#task#</Hashtag>
																	</ReadValue>
																	<ReadValue>
																		<Parameters>
																			<Row>1</Row>
																			<Column>1</Column>
																		</Parameters>
																		<Hashtag>#task2#</Hashtag>
																	</ReadValue>
																</Actions>
															</SelectTaskV2>
															<SelectDate>
																<Actions>
																	<Today></Today>
																</Actions>
															</SelectDate>
															<IsDurationDisabled>
																<ExpectedResult>true</ExpectedResult>
															</IsDurationDisabled>


														</Actions>
													</Content>
													<Ok>
														<Parameters>
															<validate>true</validate>
														</Parameters>
													</Ok>
												</Actions>
											</AddTask>
											<PressKeyFromKeyBoard>
												<Actions>
													<PressEnter></PressEnter>
												</Actions>
											</PressKeyFromKeyBoard>
											<IsTaskPresent>
												<Parameters>
													<taskName>#task2#</taskName>
												</Parameters>
												<ExpectedResult>true</ExpectedResult>
											</IsTaskPresent>
											<IsTaskPresent>
												<Parameters>
													<taskName>#task#</taskName>
												</Parameters>
												<ExpectedResult>true</ExpectedResult>
											</IsTaskPresent>
										</Actions>
									</ManageTask>
									<!--Reading toolbar buttons --><!--Reading the buttons present in the lower part of the window --><!--Reading the week range in the toolbar -->
								</Actions>
							</Content>
						</Actions>
					</ReturnTxHourTrackingInNewTab>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>