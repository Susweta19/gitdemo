<?xml version="1.0" encoding="utf-8"?>
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the view of TxHourTracking module in window mode</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Alice&#160;Dupont</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<LaunchModel>
						<Parameters>
							<modelName>Hour tracking in window  mode with TxTableView</modelName>
						</Parameters>
					</LaunchModel>
					<ReturnTxHourTrackingInWindow>
						<Actions>
							<WaitForAjax></WaitForAjax>
							<Sleep>
								<Parameters>
									<timeOut>2</timeOut>
								</Parameters>
							</Sleep>
							<ReadWindowTitle>
								<ExpectedResult>Hour Tracking - Spent times</ExpectedResult></ReadWindowTitle>
							<Content>
								<Actions>
									<IsNavigationLayoutDisplayed>
										<ExpectedResult>
											true
										</ExpectedResult></IsNavigationLayoutDisplayed>
									<ManageNavigationLayout>
										<Actions>
											<IsAccordionCollapsed>
												<Parameters>
													<accordionName>My Tasks</accordionName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsAccordionCollapsed>
											<IsAccordionCollapsed>
												<Parameters>
													<accordionName>My team</accordionName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsAccordionCollapsed>
											<IsAccordionCollapsed>
												<Parameters>
													<accordionName>My Equipments</accordionName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsAccordionCollapsed></Actions>
									</ManageNavigationLayout>
									<ManageTask>
										<Actions>
											<IsAddTaskButtonPresent>
												<Parameters>
													<resourceName>Alice Dupont</resourceName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsAddTaskButtonPresent>
											<IsTaskPresent>
												<Parameters>
													<taskName>[To be planned]</taskName>
												</Parameters>
												<ExpectedResult>false</ExpectedResult>
											</IsTaskPresent></Actions>
									</ManageTask>
									<TxHourTrackingToolBar>
										<Actions>
										<ManageCommonMethods>
												<Actions>
													<CurrentShortDay>
														<Parameters></Parameters>
														<Hashtag>#day#</Hashtag>
													</CurrentShortDay>
													<CurrentMonth>
														<Parameters>
															<monthLength>3</monthLength>
														</Parameters>
														<Hashtag>#month#</Hashtag>
													</CurrentMonth>
													<GetCurrentWeekDays>
														<Parameters></Parameters>
														<Actions>
															<ElementAt>
																<Parameters>
																	<index>0</index>
																</Parameters>
																<Hashtag>#FirstDate#</Hashtag>
															</ElementAt>
															<ElementAt>
																<Parameters>
																	<index>6</index>
																</Parameters>
																<Hashtag>#LastDate#</Hashtag>
															</ElementAt>
														</Actions>
													</GetCurrentWeekDays>
													<GetCurrentWeekDaysMonth>
														<Parameters>
															<length>3</length>
														</Parameters>
														<Actions>
															<ElementAt>
																<Parameters>
																	<index>0</index>
																</Parameters>
																<Hashtag>#FirstDayMonth#</Hashtag>
															</ElementAt>
															<ElementAt>
																<Parameters>
																	<index>6</index>
																</Parameters>
																<Hashtag>#LastDayMonth#</Hashtag>
															</ElementAt>
														</Actions>
													</GetCurrentWeekDaysMonth>
													<GetCurrentWeekDaysYear>
												<Parameters></Parameters>
												<Actions>
													<ElementAt>
														<Parameters>
															<index>0</index>
														</Parameters>
														<Hashtag>#FirstDayYear#</Hashtag>
													</ElementAt>
													<ElementAt>
														<Parameters>
															<index>6</index>
														</Parameters>
														<Hashtag>#LastDayYear#</Hashtag>
													</ElementAt>
												</Actions>
											</GetCurrentWeekDaysYear>
												</Actions>
											</ManageCommonMethods>
											<IsButtonPresent>
												<Parameters>
													<buttonName>Expand / Collapse the main menu</buttonName>
												</Parameters>
											</IsButtonPresent>
											<IsButtonPresent>
												<Parameters>
													<buttonName>Open time exportation interface</buttonName>
												</Parameters>
											</IsButtonPresent>
											<IsButtonPresent>
												<Parameters>
													<buttonName>Validate view times</buttonName>
												</Parameters>
											</IsButtonPresent>
											<IsButtonPresent>
												<Parameters>
													<buttonName>Cancel view times validation</buttonName>
												</Parameters>
											</IsButtonPresent>
											<IsButtonPresent>
												<Parameters>
													<buttonName>Previous week</buttonName>
												</Parameters>
											</IsButtonPresent>
											<IsButtonPresent>
												<Parameters>
													<buttonName>Next week</buttonName>
												</Parameters>
											</IsButtonPresent>
											<IsButtonDisabled>
												<Parameters>
													<imageName>CheckObject</imageName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsButtonDisabled>
											<IsButtonDisabled>
												<Parameters>
													<imageName>UncheckObject</imageName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsButtonDisabled>
											<IsButtonDisabled>
												<Parameters>
													<imageName>clock-black</imageName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsButtonDisabled>
											<IsButtonDisabled>
												<Parameters>
													<imageName>right-black</imageName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsButtonDisabled>
											</Actions>
									</TxHourTrackingToolBar>
									<ReadAllButtons>
										<Actions>
											<ElementAt>
												<Parameters>
													<index>0</index>
												</Parameters>
												<ExpectedResult>Expand / Collapse the main menu</ExpectedResult>
											</ElementAt>
											<ElementAt>
												<Parameters>
													<index>1</index>
												</Parameters>
												<ExpectedResult>Open time exportation interface</ExpectedResult>
											</ElementAt>
											<ElementAt>
												<Parameters>
													<index>2</index>
												</Parameters>
												<ExpectedResult>Validate view times</ExpectedResult>
											</ElementAt>
											<ElementAt>
												<Parameters>
													<index>3</index>
												</Parameters>
												<ExpectedResult>Save view times</ExpectedResult>
											</ElementAt>
											<ElementAt>
												<Parameters>
													<index>4</index>
												</Parameters>
												<ExpectedResult>Cancel view times validation</ExpectedResult>
											</ElementAt>
											<ElementAt>
												<Parameters>
													<index>5</index>
												</Parameters>
												<ExpectedResult>Today: #day#, #month# #D#, #YYYY#</ExpectedResult>
											</ElementAt>
											<ElementAt>
												<Parameters>
													<index>6</index>
												</Parameters>
												<ExpectedResult>Previous week</ExpectedResult>
											</ElementAt>
											<ElementAt>
												<Parameters>
													<index>7</index>
												</Parameters>
												<ExpectedResult>Next week</ExpectedResult>
											</ElementAt>
										</Actions>
									</ReadAllButtons>
									<ReadWeekRange>
										<ExpectedResult>Mon, #FirstDayMonth# #FirstDate#, #FirstDayYear# - Sun, #LastDayMonth# #LastDate#, #LastDayYear#</ExpectedResult>
									</ReadWeekRange></Actions>									
							</Content>
							<IsButtonPresent>
								<Parameters>
									<buttonName>Close</buttonName>
								</Parameters>
								<ExpectedResult>true</ExpectedResult>
							</IsButtonPresent></Actions>
					</ReturnTxHourTrackingInWindow></Actions>
      </MainWindow>
    </Actions></TxInterface></Actions>