<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the view of Add task(s) window for a user under My Equipments</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Alice&#160;Dupont</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<LaunchModel>
						<Parameters>
							<modelName>Hour tracking in new tab  mode with TxTableView</modelName>
						</Parameters>
					</LaunchModel>
					<ReturnTxHourTrackingInNewTab>
						<Actions>
							<Content>
								<Actions>
									<ManageNavigationLayout>
										<Actions>
											<ClickOnLinkBox>
												<Parameters>
													<linkName>My Equipments</linkName>
												</Parameters>
											</ClickOnLinkBox>
											<ManageLinkBox>
												<Parameters>
													<linkName>My Equipments</linkName>
												</Parameters>
												<Actions>
													<SelectAll></SelectAll>


												</Actions>
											</ManageLinkBox>
										</Actions>
									</ManageNavigationLayout>
									<ManageTask>
										<Actions>
											<ManageCommonMethods>
												<Actions>
													<CurrentWeekFirstDay>
														<Hashtag>#day#</Hashtag>
													</CurrentWeekFirstDay>
												</Actions>
											</ManageCommonMethods>
											<IsAddTaskButtonPresent>
												<Parameters>
													<resourceName>Machine009</resourceName>
												</Parameters>
												<ExpectedResult>true</ExpectedResult>
											</IsAddTaskButtonPresent>
											<AddTask>
												<Parameters>
													<user>Machine009</user>
												</Parameters>
												<Actions>
													<WaitForAjax></WaitForAjax>
													<Content>
														<Actions>
															<ReturnTableView>
																<Actions>
																	<ReadColumName>
																		<Actions>
																			<ElementAt>
																				<Parameters>
																					<index>1</index>
																				</Parameters>
																				<ExpectedResult>Objects</ExpectedResult>
																			</ElementAt>
																			<ElementAt>
																				<Parameters>
																					<index>2</index>
																				</Parameters>
																				<ExpectedResult>Project</ExpectedResult>
																			</ElementAt>
																			<ElementAt>
																				<Parameters>
																					<index>3</index>
																				</Parameters>
																				<ExpectedResult>Zones</ExpectedResult>
																			</ElementAt>
																		</Actions>
																	</ReadColumName>
																	<UnCheckallObjects></UnCheckallObjects>
																	<CheckallObjects></CheckallObjects>
																	<Refresh></Refresh>
																</Actions>
															</ReturnTableView>
														</Actions>
													</Content>
													<IsButtonPresent>
														<Parameters>
															<buttonName>Previous tasks</buttonName>
														</Parameters>
														<ExpectedResult>true</ExpectedResult>
													</IsButtonPresent>
													<IsButtonPresent>
														<Parameters>
															<buttonName>Cancel</buttonName>
														</Parameters>
														<ExpectedResult>true</ExpectedResult>
													</IsButtonPresent>
													<IsButtonPresent>
														<Parameters>
															<buttonName>Ok</buttonName>
														</Parameters>
														<ExpectedResult>true</ExpectedResult>
													</IsButtonPresent>
												</Actions>
											</AddTask>

										</Actions>
									</ManageTask>
								</Actions>
							</Content>
						</Actions>
					</ReturnTxHourTrackingInNewTab>
				</Actions>
			</MainWindow>
		</Actions>
