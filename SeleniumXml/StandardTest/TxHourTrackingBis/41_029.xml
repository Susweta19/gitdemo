														</Parameters>
													</Ok>
												</Actions>
											</AddTask>
											<WaitForAjax></WaitForAjax>
											<TaskContextMenu>
												<Parameters>
													<rowIndex>2</rowIndex>
													<colIndex>2</colIndex>
													<taskName>Réunion</taskName>
												</Parameters>
												<Actions>
													<RemoveTask></RemoveTask>
												</Actions>
											</TaskContextMenu>

											<ManagePopUp>
												<Actions>
													<ReadPopUpMessage>
