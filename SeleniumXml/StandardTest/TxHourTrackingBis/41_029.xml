													<Content>
														<Actions>
															<SelectTask>
																<Actions>
																	<Search>
																		<Parameters>
																			<research>re</research>
																		</Parameters>
																	</Search>
																	<SelectEntitiesBox>
																		<Parameters>
																			<collection>
																				<collection>
																					<path>Réunion</path>
																				</collection>
																			</collection>
																		</Parameters>
																		<Actions>
																			<Tick></Tick>
																		</Actions>
																	</SelectEntitiesBox>
																</Actions>
															</SelectTask>
															<SelectDate>
																<Actions>
																	<Today></Today>
																</Actions>
															</SelectDate>
															<Sleep>
																<Parameters>
																	<timeOut>2</timeOut>
																</Parameters>
															</Sleep>
														</Actions>
													</Content>
													<Ok>
