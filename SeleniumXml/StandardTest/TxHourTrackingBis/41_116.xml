<?xml version="1.0" encoding="utf-8"?>
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the ability to add multiple tasks for user under My Task </testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Alice&#160;Dupont</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<LaunchModel>
						<Parameters>
							<modelName>Hour tracking in window  mode with TxTableView</modelName>
						</Parameters>
					</LaunchModel>
					<ReturnTxHourTrackingInWindow>
						<Actions>
							<Content>
								<Actions>
									<ManageTask>
										<Actions>
										<DeleteAllPresentTask></DeleteAllPresentTask>
											<IsAddTaskButtonPresent>
												<Parameters>
													<resourceName>Alice Dupont</resourceName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsAddTaskButtonPresent>
											<AddTask>
												<Parameters>
													<index>1</index>
													<user>Alice Dupont</user></Parameters>
												<Actions>
													<Content>
														<Actions>
															<SelectTaskV2>
																<Actions>
																	<CheckBox>
																		<Parameters>
																			<Row>1</Row>
																			<Column>1</Column>
																		</Parameters>
																		<Actions>
																			<Tick></Tick></Actions>
																	</CheckBox>
																	<CheckBox>
																		<Parameters>
																			<Row>2</Row>
																			<Column>1</Column>
																		</Parameters>
																		<Actions>
																			<Tick></Tick></Actions>
																	</CheckBox>
																	<ReadValue>
																		<Parameters>
																			<Row>1</Row>
																			<Column>2</Column>
																		</Parameters>
																		<Hashtag>#task1#</Hashtag>
																	</ReadValue>
																	<ReadValue>
																		<Parameters>
																			<Row>2</Row>
																			<Column>2</Column>
																		</Parameters>
																		<Hashtag>#task2#</Hashtag>
																	</ReadValue></Actions>
															</SelectTaskV2>
															<SelectDate>
																<Actions>
																	<Today></Today></Actions>
															</SelectDate>
														</Actions>
													</Content>
													<Ok>
														<Parameters>
															<validate>
																true
															</validate></Parameters>
													</Ok></Actions>
											</AddTask>
											<IsTaskPresent>
												<Parameters>
													<taskName>#task1#</taskName>
												</Parameters>
											</IsTaskPresent>
											<IsTaskPresent>
												<Parameters>
													<taskName>#task2#</taskName>
												</Parameters>
											</IsTaskPresent>
											
										</Actions>
									</ManageTask></Actions>
							</Content></Actions>
					</ReturnTxHourTrackingInWindow></Actions>
      </MainWindow>
    </Actions></TxInterface></Actions>