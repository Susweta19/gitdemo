<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>In Time Entry section, Add task through right click on user (Alice Dupont)</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Alice&#160;Dupont</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<LaunchModel>
						<Parameters>
							<modelName>Hour tracking in window mode without TxTableView</modelName>
						</Parameters>
					</LaunchModel>
					<ReturnTxHourTrackingInWindow>
						<Actions>
							<Content>
								<Actions>
									<WaitForAjax></WaitForAjax>
									<ManageNavigationLayout>
										<Actions>
											<ClickOnLinkBox>
												<Parameters>
													<linkName>My Equipments</linkName>
												</Parameters>
											</ClickOnLinkBox><ManageLinkBox><Parameters><linkName>My Equipments</linkName></Parameters><Actions><SelectAll></SelectAll></Actions></ManageLinkBox>
										</Actions>
									</ManageNavigationLayout>
									<ManageTask>
										<Actions>
											<DeleteAllPresentTask></DeleteAllPresentTask>
											
											<TaskContextMenu>
												<Parameters>
													<taskName>Machine005</taskName>
												</Parameters>
												<Actions>
													<AddTask>
														<Actions>
														<WaitForAjax></WaitForAjax>
													<ReadHeader>
														<ExpectedResult>Add task(s)</ExpectedResult></ReadHeader>
													<Content>
														<Actions>
															<IsButtonDisabled>
																<Parameters>
																	<buttonName>Ok</buttonName>
																</Parameters>
																<ExpectedResult>true</ExpectedResult>
															</IsButtonDisabled>
															<SelectTask>
																<Actions>
																	<SelectEntitiesBox>
																		<Parameters>
																			<collection>
																				<collection>
																					<path>T-0599</path>
																				</collection>
																			</collection>
																		</Parameters>
																		<Actions>
																			<Tick></Tick>
																		</Actions>
																	</SelectEntitiesBox>
																</Actions>
															</SelectTask>
															<SelectDate>
																<Actions>
																	<Today></Today>
																</Actions>
															</SelectDate>
															<IsButtonDisabled>
																<Parameters>
																	<buttonName>Ok</buttonName>
																</Parameters>
																<ExpectedResult>false</ExpectedResult>
															</IsButtonDisabled>
														</Actions>
													</Content>
													<Ok>
														<Parameters>
															<validate>
																true
															</validate></Parameters>
													</Ok></Actions>
													</AddTask></Actions>
											</TaskContextMenu>
											<PressKeyFromKeyBoard><Actions><PressEnter /></Actions></PressKeyFromKeyBoard>
											<IsTaskPresent>
												<Parameters>
													<taskName>T-0599</taskName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsTaskPresent>

										</Actions>
									</ManageTask></Actions>
							</Content>
							<Cancel></Cancel>
						</Actions>
					</ReturnTxHourTrackingInWindow>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>