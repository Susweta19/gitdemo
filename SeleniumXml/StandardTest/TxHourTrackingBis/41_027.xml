<?xml version="1.0" encoding="utf-8"?>
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>In Time Entry section, Add task through right click on user (Alice Dupont)</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Alice&#160;Dupont</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<LaunchModel>
						<Parameters>
							<modelName>Hour tracking in window mode without TxTableView</modelName>
						</Parameters>
					</LaunchModel>
					<ReturnTxHourTrackingInWindow>
						<Actions>
							<Content>
								<Actions>
									<WaitForAjax></WaitForAjax>
									<ManageTask>
										<Actions>
											<DeleteAllPresentTask></DeleteAllPresentTask>
											
											<TaskContextMenu>
												<Parameters>
													<taskName>Alice Dupont</taskName>
												</Parameters>
												<Actions>
													<AddTask>
														<Actions>
														<WaitForAjax></WaitForAjax>
													<ReadHeader>
														<ExpectedResult>Add task(s)</ExpectedResult></ReadHeader>
													<Content>
														<Actions>
															<ManageCommonMethods>
															<Actions>
															<GetCurrentWeekDays>
															<Parameters>
															</Parameters>
															<Actions>
															<ElementAt>
															<Parameters>
															<index>0</index>
															</Parameters>
															<Hashtag>#date#</Hashtag>
															</ElementAt>
															</Actions>
															</GetCurrentWeekDays>
															</Actions>
															</ManageCommonMethods>
															<SelectTask>
																<Actions>
																	<Search>
																		<Parameters>
																			<research>re</research>
																		</Parameters>
																	</Search>
																	<SelectEntitiesBox>
																		<Parameters>
																			<collection>
																				<collection>
																					<path>Réunion</path>
																				</collection>
																			</collection>
																		</Parameters>
																		<Actions>
																			<Tick></Tick>
																		</Actions>
																	</SelectEntitiesBox>
																</Actions>
															</SelectTask>
															<SelectDate>
																<Actions>
																	<Day>
																		<Parameters>
																			<day>#date#</day>
																		</Parameters>
																	</Day></Actions>
															</SelectDate>
															<WriteDuration>
																<Actions>
																	<Write>
																		<Parameters>
																			<value>2</value>
																		</Parameters>
																	</Write></Actions>
															</WriteDuration>
														</Actions>
													</Content>
													<Ok>
														<Parameters>
															<validate>
																true
															</validate></Parameters>
													</Ok></Actions>
													</AddTask></Actions>
											</TaskContextMenu>
											<PressKeyFromKeyBoard><Actions><PressEnter /></Actions></PressKeyFromKeyBoard>
											<IsTaskPresent>
												<Parameters>
													<taskName>Réunion</taskName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsTaskPresent>
										</Actions>
									</ManageTask></Actions>
							</Content>
							<Close></Close>
							
						</Actions>
					</ReturnTxHourTrackingInWindow>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>