<?xml version="1.0" encoding="utf-8"?>
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the Functionality of Remove Task function for a task under My team</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Alice&#160;Dupont</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<LaunchModel>
						<Parameters>
							<modelName>Hour tracking in window  mode with TxTableView</modelName>
						</Parameters>
					</LaunchModel>
					<ReturnTxHourTrackingInWindow>
						<Actions>
							<Content>
								<Actions>
								<ManageNavigationLayout>
										<Actions>
											<ClickOnLinkBox>
												<Parameters>
													<linkName>My team</linkName>
												</Parameters>
											</ClickOnLinkBox>
											<ManageLinkBox>
												<Parameters>
													<linkName>My team</linkName>
												</Parameters>
												<Actions>
													<SelectAll></SelectAll>
													
													
												</Actions>
											</ManageLinkBox></Actions>
									</ManageNavigationLayout>
									<ManageTask>
										<Actions>
											<ManageCommonMethods>
												<Actions>
													<CurrentWeekFirstDay>
														<Hashtag>#day#</Hashtag></CurrentWeekFirstDay>
												</Actions>
											</ManageCommonMethods>
											<DeleteAllPresentTask></DeleteAllPresentTask>
											<AddTask>
												<Parameters>
													<index>1</index>
													<user>Luc Bariol</user></Parameters>
												<Actions>
													<Content>
														<Actions>
															<SelectTaskV2>
																<Actions>
																	<CheckBox>
																		<Parameters>
																			<Row>1</Row>
																			<Column>1</Column>
																		</Parameters>
																		<Actions>
																			<Tick></Tick></Actions>
																	</CheckBox>
																	<ReadValue>
																		<Parameters>
																			<Row>1</Row>
																			<Column>1</Column>
																		</Parameters>
																		<Hashtag>#task#</Hashtag>
																	</ReadValue></Actions>
															</SelectTaskV2>
															<SelectDate>
																<Actions>
																	<Day>
																		<Parameters>
																			<day>#day#</day>
																		</Parameters>
																	</Day>
																</Actions>
															</SelectDate>
														</Actions>
													</Content>
													<Ok>
														<Parameters>
															<validate>
																true
															</validate></Parameters>
													</Ok></Actions>
											</AddTask>
											<IsTaskPresent>
												<Parameters>
													<taskName>#task#</taskName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsTaskPresent>
											<TaskContextMenu>
												<Parameters>
													<colIndex>3</colIndex>
													<taskName>#task#</taskName>
												</Parameters>
												<Actions>
													<IsFunctionDisabled>
														<Parameters>
															<functionName>Remove Task</functionName>
														</Parameters>
														<ExpectedResult>false</ExpectedResult>
													</IsFunctionDisabled>
													<RemoveTask></RemoveTask>

												</Actions>
											</TaskContextMenu>
											<ManagePopUp>
												<Actions>
													<CloseConfirmationPopup>
														<Parameters>
															<result>
																true
															</result>
														</Parameters>
													</CloseConfirmationPopup></Actions>
											</ManagePopUp>
											<WaitForAjax></WaitForAjax>
											<IsTaskPresent>
												<Parameters>
													<taskName>#task#</taskName>
												</Parameters>
												<ExpectedResult>false</ExpectedResult>
											</IsTaskPresent>
										</Actions>
									</ManageTask></Actions>
							</Content></Actions>
					</ReturnTxHourTrackingInWindow></Actions>
      </MainWindow>
    </Actions></TxInterface></Actions>