          				</SelectObject>
          				<SelectObject>
          					<Parameters>
          						<objectName>Object2</objectName>
          					</Parameters>
          				</SelectObject>
          				<SelectObject>
          					<Parameters>
          						<objectName>Object3</objectName>
          					</Parameters>
          				</SelectObject>
          				<Sleep>
          					<Parameters>
          						<timeOut>5</timeOut>
          					</Parameters>
          				</Sleep>
          				<PerformActions>
          					<Actions>
          						<WFAction>
          							<Parameters>
          								<actionName>Data writting</actionName>
          							</Parameters>
          						</WFAction>
          						<ManagePopUp>
          							<Actions>
          								<ReadConfirmationPopUpMessage>
          									<Actions>
          										<ElementAt>
          											<Parameters>
          												<index>
          													0
          												</index>
          											</Parameters>
          											<ExpectedResult>Do you want to run the action 'Data writting' on the 3 selected Objects?</ExpectedResult>
          										</ElementAt></Actions>
          								</ReadConfirmationPopUpMessage>
          								<CloseConfirmationPopup>
          									<Parameters>
          										<result>true</result>
          									</Parameters>
          								</CloseConfirmationPopup>
          							</Actions>
          						</ManagePopUp>
          						</Actions>
          				</PerformActions>
          				<WaitForAjax></WaitForAjax>
          						<ManagePopUp>
          							<Actions>
          								<ReadHeader>
          									<ExpectedResult>Actions progress</ExpectedResult></ReadHeader>
          								<DoNotAskAgain>
          									<Actions>
          										<Read>
          											<ExpectedResult>false</ExpectedResult></Read>
          										<Tick></Tick>
          										<Read>
          											<ExpectedResult>
          												true
          											</ExpectedResult></Read></Actions>
          								</DoNotAskAgain>
          								<Next></Next></Actions>
          						</ManagePopUp></Actions>
