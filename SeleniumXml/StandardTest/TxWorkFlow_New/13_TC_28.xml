          			<Parameters><!--Action 4 - Workflow on linked objects with table-->
          				<actionName>Action 3 - Workflow on linked objects without table</actionName>
          			</Parameters>
          		</WFAction>
          		
          		
          	</Actions>
          </ReturnWFPromote>
          <ReturnLinkedObject>
          	<Actions>
          		<Content>
          			<Actions>
          				<WaitForAjax></WaitForAjax>
          				<ReadActionList>
          					<Actions>
          						<ElementAt>
          							<Parameters>
          								<index>0</index>
