<?xml version="1.0" encoding="utf-8"?>
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Selecting a date in the calendar</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrateur</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<TxHourTracking>
						<Actions>
							<Content>
								<Actions>
									<Sleep>
										<Parameters>
											<timeOut>2</timeOut>
										</Parameters>
									</Sleep>
									<ManageNavigationLayout>
										<Actions>
											<SelectDate>
												<Actions>
													<SelectPreviousDay>
														<Parameters>
															<numDay>7</numDay>
														</Parameters>
													</SelectPreviousDay>
												</Actions>
											</SelectDate>
										</Actions>
									</ManageNavigationLayout>
									<ManageTask>
										<Actions>
											<Sleep>
												<Parameters>
													<timeOut>2</timeOut>
												</Parameters>
											</Sleep>
										</Actions>
									</ManageTask>
								</Actions>
							</Content>
							<Cancel></Cancel>
						</Actions>
					</TxHourTracking>
					<Logout>
						<Actions>
							<ReadConfirmationPopUpMessage>
								<Actions>
									<ElementAt>
										<Parameters>
											<index>0</index>
										</Parameters>
										<ExpectedResult>Do you really want to quit TEEXMA?</ExpectedResult>
									</ElementAt></Actions>
							</ReadConfirmationPopUpMessage>
							<CloseConfirmationPopup>
								<Parameters>
									<result>true</result>
								</Parameters>
							</CloseConfirmationPopup></Actions>
					</Logout>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>