<?xml version="1.0" encoding="utf-8"?>
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check to add time in two way for a particular selected task</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrateur</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<Sleep>
						<Parameters>
							<timeOut>5</timeOut>
						</Parameters>
					</Sleep>
					<TxHourTracking>
						<Actions>
							<Content>
								<Actions>
									<ManageNavigationLayout>
										<Actions>
											<ClickOnLinkBox>
												<Parameters>
													<linkName>Machine</linkName>
												</Parameters>
											</ClickOnLinkBox>
										</Actions>
									</ManageNavigationLayout>
									<Sleep>
										<Parameters>
											<timeOut>4</timeOut>
										</Parameters>
									</Sleep>
									<ManageTask>
										<Actions>
											<DeleteAllPresentTask></DeleteAllPresentTask>
											<WaitForSaving></WaitForSaving>
											<AddTask>
												<Parameters>
													<user>Machine B</user>
												</Parameters>
												<Actions>
													<Content>
														<Actions>
															<SelectTask>
																<Actions>
																	<SelectEntityBox>
																		<Parameters>
																			<nodes>
																				<path>Actif</path>
																				<path>Tâche 3</path>
																			</nodes>
																		</Parameters>
																		<Actions>
																			<Tick />
																		</Actions>
																	</SelectEntityBox>
																</Actions>
															</SelectTask>
															<ManageCommonMethods>
																<Actions>
																	<CurrentMonth>
																		<Parameters>
																			<monthLength>3</monthLength></Parameters>
																		<Hashtag>#MMMM#</Hashtag>
																	</CurrentMonth></Actions>
															</ManageCommonMethods>
															<SelectDate>
																<Actions>
																	<Month>
																		<Parameters>
																			<month>#MMMM#</month>
																		</Parameters>
																	</Month>
																	<Day>
																		<Parameters>
																			<day>#D#</day>
																		</Parameters>
																	</Day>
																</Actions>
															</SelectDate>
														</Actions>
													</Content>
													<Ok>
														<Parameters></Parameters>
													</Ok>
												</Actions>
											</AddTask>
											<WaitForSaving></WaitForSaving>
											<IsTaskPresent>
												<Parameters>
													<taskName>Tâche 3</taskName>
												</Parameters>
												<ExpectedResult>
													true
												</ExpectedResult>
											</IsTaskPresent>
											<Normal>
												<Parameters>
													<taskName>Tâche 3</taskName>
													<colNum>4</colNum>
												</Parameters>
												<Actions>
													<Write>
														<Parameters>
															<value>2</value>
														</Parameters>
													</Write>
													<PressEnter />
												</Actions>
											</Normal>
											<WaitForSaving></WaitForSaving>
										</Actions>
									</ManageTask>
									<ManageTask>
										<Actions>
											<Maintenance>
												<Parameters>
													<taskName>Tâche 3</taskName>
													<colNum>4</colNum>
												</Parameters>
												<Actions>
													<Write>
														<Parameters>
															<value>3</value>
														</Parameters>
													</Write>
													<PressEnter />
												</Actions>
											</Maintenance>
										</Actions>
									</ManageTask>
								</Actions>
							</Content>
							<Cancel></Cancel>
							
						</Actions>
					</TxHourTracking>
					<Logout>
						<Actions>
							<ReadConfirmationPopUpMessage>
								<Actions>
									<ElementAt>
										<Parameters>
											<index>0</index>
										</Parameters>
										<ExpectedResult>Do you really want to quit TEEXMA?</ExpectedResult>
									</ElementAt></Actions>
							</ReadConfirmationPopUpMessage>
							<CloseConfirmationPopup>
								<Parameters>
									<result>true</result>
								</Parameters>
							</CloseConfirmationPopup></Actions>
					</Logout>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>