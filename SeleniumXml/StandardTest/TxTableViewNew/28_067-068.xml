	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrateur</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
