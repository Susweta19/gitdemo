<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Check the availability of the 'Min_Max_Mean' column. </testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrateur</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>Portals</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>For_Check_Test</path>
											</nodes>
										</Parameters>
									</SelectEntity>
								</Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<Sleep>
						<Parameters>
							<timeOut>8</timeOut>
						</Parameters>
					</Sleep>
					<TableViewInWidget>
						<Actions>
							<ReadColumName>
								<Actions>
									<ElementAt>
										<Parameters>
											<index>9</index>
