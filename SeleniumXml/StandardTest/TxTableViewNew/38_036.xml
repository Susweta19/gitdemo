													</Write></Actions>
											</EditMin>
											<EditMax>
												<Actions>
													<Write>
														<Parameters>
															<value>15</value>
														</Parameters>
													</Write></Actions>
											</EditMax></Actions>
									</Content>
									<Ok>
										<Parameters>
											<validate>true</validate></Parameters>
									</Ok></Actions>
							</EditDecimal2>
							
							
							<ReadValue>
								<Parameters>
									<Row>1</Row>
									<Column>9</Column>
								</Parameters>
								<ExpectedResult>from 5 to 15 - Mean: 10</ExpectedResult>
							</ReadValue></Actions>
					</TableViewInWidget>
