<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Check the inability to open a locked file</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associativeteststart</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrateur</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>WebDAV_Test</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
              	<Actions>
              		<SelectEntity>
              			<Parameters>
              				<nodes>
              					<path>Extensions_Test</path></nodes>
              			</Parameters>
              		</SelectEntity></Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <LaunchModel>
          	<Parameters>
          		<modelName>Open Full WebDAV</modelName>
          	</Parameters>
          </LaunchModel>
          <ReturnEditFileWindow>
          	<Actions>
          		<Content>
          			<Actions>
          				<WaitForAjax></WaitForAjax>
          				<Value>
          					<WEGLink>
          						<Parameters>
          							<colIndex>2</colIndex>
          							<rowIndex>2</rowIndex>
          						</Parameters>
          						<Actions>
          							<GetText>
          								<ExpectedResult>excel_test.xlsx</ExpectedResult></GetText>
          							<Click></Click></Actions>
          					</WEGLink>
          				</Value>
          				<Sleep>
          					<Parameters>
          						<timeOut>2</timeOut>
          					</Parameters>
          				</Sleep>
          				<ManageJavaScriptAlert>
          					<Actions>
          						<Accept></Accept></Actions>
          				</ManageJavaScriptAlert>
          				</Actions>
          		</Content>
          		<Refresh></Refresh>
          		<Content>
          			<Actions>
          				<WaitForAjax></WaitForAjax>
          				<Value>
          					<WEGLink>
          						<Parameters>
          							<colIndex>2</colIndex>
          							<rowIndex>2</rowIndex>
          						</Parameters>
          						<Actions>
          							<GetText>
          								<ExpectedResult>excel_test.xlsx</ExpectedResult></GetText>
          							<Click></Click></Actions>
          					</WEGLink>
          				</Value>
          				<Sleep>
          					<Parameters>
          						<timeOut>2</timeOut>
          					</Parameters>
          				</Sleep>
          				<ManagePopUp>
          					<Actions>
          						<ReadPopUpMessage>
          							<Actions>
          								<ElementAt>
          									<Parameters>
          										<index>0</index>
          									</Parameters>
          									<ExpectedResult>The file "WebDAV_File\excel_test.xlsx" is already locked.</ExpectedResult>
          								</ElementAt></Actions>
          						</ReadPopUpMessage>
          						<ClosePopUp>
          							<Parameters>
          								<result>true</result>
          							</Parameters>
          						</ClosePopUp></Actions>
          				</ManagePopUp></Actions>
          		</Content>
          		<Close></Close></Actions>
          </ReturnEditFileWindow>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>