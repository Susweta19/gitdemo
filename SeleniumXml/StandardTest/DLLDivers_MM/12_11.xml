<?xml version="1.0" encoding="utf-8"?>
<!--Conditional Renaming-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>After selecting an object from the tree, Tick an object for the attribute 'Single List for Renaming'_If we select SLR_1, the object will be renaming 'Renaming_Through_SLR1'_If we select SLR_2, the object will be renaming 'Renaming_Through_SLR2'._If we select SLR_3, the object will be renaming 'Renaming_Through_SLR3'</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Message>Associative test start</Message>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_TxStrGen</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <EntitytPresence>
                    <Parameters>
                      <nodes>
                        <path>Renaming_Through_SLR1</path>
                      </nodes>
                      <presence>false</presence>
                    </Parameters>
                  </EntitytPresence>
                  <EntitytPresence>
                    <Parameters>
                      <nodes>
                        <path>New1</path>
                      </nodes>
                      <presence>true</presence>
                    </Parameters>
                  </EntitytPresence>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New1</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <!--we select SLR_3 for conditionnal renaming-->
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <GetLink>
                    <Parameters>
                      <attrId>273</attrId>
                    </Parameters>
                    <Actions>
                      <SelectEntityBox>
                        <Parameters>
                          <nodes>
                            <path>SLR_1</path>
                          </nodes>
                        </Parameters>
                        <Actions>
                          <Tick />
                        </Actions>
                      </SelectEntityBox>
                    </Actions>
                  </GetLink>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <LaunchModel>
            <Parameters>
              <modelName>Conditional naming</modelName>
            </Parameters>
          </LaunchModel>
          <NavigationTree>
            <Actions>
              <EntityTree>
                <Actions>
                  <EntitytPresence>
                    <Parameters>
                      <nodes>
                        <path>New1</path>
                      </nodes>
                      <presence>false</presence>
                    </Parameters>
                  </EntitytPresence>
                  <EntitytPresence>
                    <Parameters>
                      <nodes>
                        <path>Renaming_Through_SLR1</path>
                      </nodes>
                      <presence>true</presence>
                    </Parameters>
                  </EntitytPresence>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <GetLink>
                    <Parameters>
                      <attrId>273</attrId>
                    </Parameters>
                    <Actions>
                      <Search>
                        <Parameters>
                          <research>SLR_2</research>
                        </Parameters>
                      </Search>
                      <SelectEntityBox>
                        <Parameters>
                          <nodes>
                            <path>SLR_2</path>
                          </nodes>
                        </Parameters>
                        <Actions>
                          <Tick />
                        </Actions>
                      </SelectEntityBox>
                    </Actions>
                  </GetLink>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <LaunchModel>
            <Parameters>
              <modelName>Conditional naming</modelName>
            </Parameters>
          </LaunchModel>
          <NavigationTree>
            <Actions>
              <EntityTree>
                <Actions>
                  <EntitytPresence>
                    <Parameters>
                      <nodes>
                        <path>Renaming_Through_SLR1</path>
                      </nodes>
                      <presence>false</presence>
                    </Parameters>
                  </EntitytPresence>
                  <EntitytPresence>
                    <Parameters>
                      <nodes>
                        <path>Renaming_Through_SLR2</path>
                      </nodes>
                      <presence>true</presence>
                    </Parameters>
                  </EntitytPresence>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <GetLink>
                    <Parameters>
                      <attrId>273</attrId>
                    </Parameters>
                    <Actions>
                      <Search>
                        <Parameters>
                          <research>SLR_3</research>
                        </Parameters>
                      </Search>
                      <SelectEntityBox>
                        <Parameters>
                          <nodes>
                            <path>SLR_3</path>
                          </nodes>
                        </Parameters>
                        <Actions>
                          <Tick />
                        </Actions>
                      </SelectEntityBox>
                    </Actions>
                  </GetLink>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <LaunchModel>
            <Parameters>
              <modelName>Conditional naming</modelName>
            </Parameters>
          </LaunchModel>
          <NavigationTree>
            <Actions>
              <EntityTree>
                <Actions>
                  <EntitytPresence>
                    <Parameters>
                      <nodes>
                        <path>Renaming_Through_SLR2</path>
                      </nodes>
                      <presence>false</presence>
                    </Parameters>
                  </EntitytPresence>
                  <EntitytPresence>
                    <Parameters>
                      <nodes>
                        <path>Renaming_Through_SLR3</path>
                      </nodes>
                      <presence>true</presence>
                    </Parameters>
                  </EntitytPresence>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>