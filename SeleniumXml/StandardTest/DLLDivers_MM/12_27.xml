          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>Test</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <SwitchReadWrite />
              <AddObject>
                <Parameters />
                <Actions>
                  <Content>
                    <Actions>
                      <PublishDocumentFile>
