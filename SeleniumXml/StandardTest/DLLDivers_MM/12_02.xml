<?xml version="1.0" encoding="utf-8"?>
<!--Check the availability of the function 'Add_And_Link_Objects' -->
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>After launching "APP_TO_AALOs" a new object will be added to "TEST_TO_3" and will be linked to the selected object</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Message>Associative test start</Message>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_TO_3</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_TO_1</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>1_New_1</path>
											</nodes>
										</Parameters>
									</SelectEntity>
								</Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<Sleep>
						<Parameters>
							<timeOut>3</timeOut>
						</Parameters>
					</Sleep>
					<LaunchModel>
						<Parameters>
							<modelName>APP_TO_AALOs</modelName>
						</Parameters>
					</LaunchModel>
					<Sleep>
						<Parameters>
							<timeOut>5</timeOut>
						</Parameters>
					</Sleep>
					<ReturnEditForm>
						<Actions>
							<Content>
								<Actions>
									<Write>
										<DTText>
											<Parameters>
												<id>3368</id>
												<value>
													<value>New</value>
												</value>
											</Parameters>
										</DTText>

									</Write>
									<Write>
										<DTText>
											<Parameters>
												<id>3371</id>
												<value>
													<value>6</value>
												</value>
											</Parameters>
										</DTText>

									</Write>
								</Actions>
							</Content>
							<Ok>
								<Parameters>
									<validate>true</validate>
								</Parameters>
							</Ok>
						</Actions>
					</ReturnEditForm>
					<Sleep>
						<Parameters>
							<timeOut>5</timeOut>
						</Parameters>
					</Sleep>
					<NotificationManager>
						<Actions>

							<ReadNotificationFromNotificationBar>
								<Parameters>
									<datanumber>0</datanumber>
									<expand>true</expand>
								</Parameters><!--The Object "New_6_19/11/2020" of type "TEST_TO_3" 
									was successfully added and linked to the Object "1_New_1".Just now -->
								<ExpectedResult>The Object "New_6_#CurrentDate#" of type "TEST_TO_3" was successfully added and linked to the Object "1_New_1".Just now</ExpectedResult>

							</ReadNotificationFromNotificationBar>
						</Actions>
					</NotificationManager>

					<ReadForm>
						<Actions>
							<Read>
								<WEGLink>
									<Parameters>
										<attrId>1358</attrId>
									</Parameters>
									<Actions>
										<GetText>
											<ExpectedResult>New_6_#CurrentDate#</ExpectedResult>
										</GetText>
										<Click />
									</Actions>
								</WEGLink>
							</Read>
						</Actions>
					</ReadForm>
					<Sleep>
						<Parameters>
							<timeOut>2</timeOut>
						</Parameters>
					</Sleep>
					<ReadBanner>
						<ExpectedResult>New_6_#CurrentDate#</ExpectedResult>
					</ReadBanner>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Read>
										<ExpectedResult>TEST_TO_3</ExpectedResult>
									</Read>
								</Actions>
							</SelectOT>
						</Actions>
					</NavigationTree>
					<ReadForm>
						<Actions>
							<Read>
								<WEGLink>
									<Parameters>
										<attrId>1357</attrId>
									</Parameters>
									<Actions>
										<GetText>
											<ExpectedResult>1_New_1</ExpectedResult>
										</GetText>
										<Click></Click>
									</Actions>
								</WEGLink>
							</Read>
						</Actions>
					</ReadForm>
					<ReadBanner>
						<ExpectedResult>1_New_1</ExpectedResult>
					</ReadBanner>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Read>
										<ExpectedResult>TEST_TO_1</ExpectedResult>
									</Read>
								</Actions>
							</SelectOT>
						</Actions>
					</NavigationTree>
				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>