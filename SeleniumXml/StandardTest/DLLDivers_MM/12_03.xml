<?xml version="1.0" encoding="utf-8"?>
<!--Check the availability of the function 'Link_Associative_Objects'-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription >After  launching "App_To_AddObject" a new object will be added</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Message>Associative test start</Message>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_TO_1</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters><nodes><path>1_New_1</path></nodes></Parameters>
									</SelectEntity></Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<Sleep>
						<Parameters>
							<timeOut>3</timeOut>
						</Parameters>
					</Sleep>
					<LaunchModel>
						<Parameters>
							<modelName>App_To_AddObject</modelName>
						</Parameters>
					</LaunchModel>
					<ReturnEditForm>
						<Actions>
							<Content>
								<Actions>
									<Write>
										<DTText>
											<Parameters>
												<id>3378</id>
												<value>
													<value>CreatedForAddObject</value>
												</value>
											</Parameters>
										</DTText>
										
									</Write>
									<Write>
										<DTText>
											<Parameters>
												<id>3379</id>
												<value>
													<value>101</value>
												</value>
											</Parameters>
										</DTText>
										
									</Write></Actions>
							</Content>
							<Ok>
								<Parameters>
									<validate>true</validate></Parameters>
							</Ok></Actions>
					</ReturnEditForm>
					<NotificationManager>
						<Actions>
							<ReadNotificationFromNotificationBar>
								<Parameters>
									<datanumber>0</datanumber>
								</Parameters>
								<ExpectedResult>The Object "CreatedForAddObject_101_#CurrentDate#" was successfully added.Just now</ExpectedResult>
							</ReadNotificationFromNotificationBar>
						</Actions>
					</NotificationManager>
					<NavigationTree>
						<Actions><SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_TO_2</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_TO_1</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<SelectEntity>
										<Parameters>
											<nodes>
												<path>CreatedForAddObject_101_#CurrentDate#</path></nodes>
										</Parameters>
									</SelectEntity></Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<ReadForm>
						<Actions>
							<Read>
								<DTText>
									<Parameters>
										<attrId>3378</attrId>
									</Parameters>
									<ExpectedResult>
										<value>CreatedForAddObject</value>
									</ExpectedResult>
								</DTText>
								
							</Read>
							<Read>
								<DTText>
									<Parameters>
										<attrId>3379</attrId>
									</Parameters>
									<ExpectedResult>
										<value>101</value>
									</ExpectedResult>
								</DTText>
								
							</Read>
							<Read>
								<DTDate>
									<Parameters>
										<attrId>3377</attrId>
									</Parameters>
									<ExpectedResult>
										<value>#CurrentDate#|#MM#/#DD#/#YYYY#</value>
									</ExpectedResult>
								</DTDate>
								
								
							</Read>
						</Actions>
					</ReadForm>
				</Actions>
			</MainWindow></Actions></TxInterface></Actions>