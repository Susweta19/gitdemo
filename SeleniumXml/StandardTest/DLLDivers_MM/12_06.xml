<?xml version="1.0" encoding="utf-8"?>
<!--Check the availability of the function 'Link_Associative_Objects'-->
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription >After launching "ForDeleteObject" selected object will be deleted</testDescription>
			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Message>Associative test start</Message>
		<Actions>
			<Login>
				<Parameters>
					<login>Administrator</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>
					<NavigationTree>
						<Actions>
							<SelectOT>
								<Actions>
									<Select>
										<Parameters>
											<value>TEST_TO_4</value>
										</Parameters>
									</Select>
								</Actions>
							</SelectOT>
							<EntityTree>
								<Actions>
									<EntitytPresence>
										<Parameters>
											<nodes>
												<path>4_New_4</path></nodes>
											<presence>true</presence>
										</Parameters>
									</EntitytPresence>
									<SelectEntity>
										<Parameters><nodes><path>4_New_4</path></nodes></Parameters>
									</SelectEntity></Actions>
							</EntityTree>
						</Actions>
					</NavigationTree>
					<LaunchModel>
						<Parameters>
							<modelName>ForDeleteObject</modelName>
						</Parameters>
					</LaunchModel>
					<NavigationTree>
						<Actions>
							<Sleep>
								<Parameters>
									<timeOut>2</timeOut>
								</Parameters>
							</Sleep>
							<EntityTree>
								<Actions>
									<EntitytPresence>
										<Parameters>
											<nodes>
												<path>4_New_4</path></nodes>
											<presence>false</presence>
										</Parameters>
									</EntitytPresence></Actions>
							</EntityTree></Actions>
					</NavigationTree>
				</Actions>
			</MainWindow></Actions></TxInterface></Actions>