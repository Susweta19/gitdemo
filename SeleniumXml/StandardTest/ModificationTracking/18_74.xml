<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>the history will be sorted out by the date</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Tracking</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <NavigationTree>
            <Actions>
              <TreeContextMenu>
                <Actions>
                  <ModificationsHistory>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                    <Actions>
                      <Content>
                        <Actions>
                          <ModificationDate>
                            <Actions>
                              <GetCurrentMonth>
                                <Hashtag>#MonthForCalendar#</Hashtag>
                              </GetCurrentMonth>
                              <FromDate>
                                <Actions>
                                  <Sleep>
                                    <Parameters>
                                      <timeOut>5</timeOut>
                                    </Parameters>
                                  </Sleep>
                                  <Year>
                                    <Parameters>
                                      <year>#YYYY#</year>
                                    </Parameters>
                                  </Year>
                                  <Month>
                                    <Parameters>
                                      <month>#MonthForCalendar#</month>
                                    </Parameters>
                                  </Month>
                                  <Day>
                                    <Parameters>
                                      <day>#D#</day>
                                    </Parameters>
                                  </Day>
                                </Actions>
                              </FromDate>
                              <ToDate>
                                <Actions>
                                  <Year>
                                    <Parameters>
                                      <year>#YYYY#</year>
                                    </Parameters>
                                  </Year>
                                  <Month>
                                    <Parameters>
                                      <month>#MonthForCalendar#</month>
                                    </Parameters>
                                  </Month>
                                  <Day>
                                    <Parameters>
                                      <day>#D#</day>
                                    </Parameters>
                                  </Day>
                                </Actions>
                              </ToDate>
                              <Ok>
                              </Ok>
                            </Actions>
                          </ModificationDate>
                        </Actions>
                      </Content>
                    </Actions>
                  </ModificationsHistory>
                </Actions>
              </TreeContextMenu>
            </Actions>
          </NavigationTree>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>