<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDependencies>18_05.xml</testDependencies>
        <testDescription>Modify the first values in attribute DateAndTime and we can track it</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Tracking</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <Date>
                    <Parameters>
                      <attrId>158</attrId>
                    </Parameters>
                    <Actions>
                      <Hour>
                        <Parameters>
                          <hour>10</hour>
                        </Parameters>
                      </Hour>
                      <Minute>
                        <Parameters>
                          <minute>35</minute>
                        </Parameters>
                      </Minute>
                      <Year>
                        <Parameters>
                          <year>2016</year>
                        </Parameters>
                      </Year>
                      <Month>
                        <Parameters>
                          <month>Jun</month>
                        </Parameters>
                      </Month>
                      <Day>
                        <Parameters>
                          <day>13</day>
                        </Parameters>
                      </Day>
                    </Actions>
                  </Date>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <NavigationTree>
            <Actions>
              <TreeContextMenu>
                <Actions>
                  <ModificationsHistory>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                    <Actions>
                      <Content>
                        <Actions>
                          <ResultsList>
                            <Actions>
                              <ReadEntityRow>
                                <Parameters>
                                  <nodes>
                                    <path>Date&amp;Time</path>
                                  </nodes>
                                  <ExpectedValue>
                                    <collection>13/06/2016 10:35:00</collection>
                                  </ExpectedValue>
                                </Parameters>
                              </ReadEntityRow>
                            </Actions>
                          </ResultsList>
                        </Actions>
                      </Content>
                    </Actions>
                  </ModificationsHistory>
                </Actions>
              </TreeContextMenu>
            </Actions>
          </NavigationTree>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>