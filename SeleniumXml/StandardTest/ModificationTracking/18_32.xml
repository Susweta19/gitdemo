<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Modify the first values in attribute Document_File and we can track it</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Tracking</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <PublishDocumentFile>
                    <!--Chnage the document file-->
                    <Parameters>
                      <attrId>160</attrId>
                      <pathFile>#DocPath#Picture4.jpg</pathFile>
                    </Parameters>
                  </PublishDocumentFile>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <NavigationTree>
            <Actions>
              <TreeContextMenu>
                <Actions>
                  <ModificationsHistory>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                    <Actions>
                      <Content>
                        <Actions>
                          <ResultsList>
                            <Actions>
                              <ExpandCollapseEntity>
                                <Parameters>
                                  <nodes>
                                    <path>Administrator</path>
                                  </nodes>
                                </Parameters>
                              </ExpandCollapseEntity>
                              <ExpandCollapseEntity>
                                <Parameters>
                                  <nodes>
                                    <path>Administrator</path>
                                  </nodes>
                                </Parameters>
                              </ExpandCollapseEntity>
                              <ReadEntityRow>
                                <Parameters>
                                  <nodes>
                                    <path>Document-File</path>
                                  </nodes>
                                  <ExpectedValue>
                                    <collection>TEST_Document.txt</collection>
                                    <collection>Picture4.jpg</collection>
                                  </ExpectedValue>
                                </Parameters>
                              </ReadEntityRow>
                            </Actions>
                          </ResultsList>
                        </Actions>
                      </Content>
                    </Actions>
                  </ModificationsHistory>
                </Actions>
              </TreeContextMenu>
            </Actions>
          </NavigationTree>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>