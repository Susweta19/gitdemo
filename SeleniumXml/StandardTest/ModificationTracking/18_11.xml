<?xml version="1.0" encoding="utf-8"?>
<Actions xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <TestHeader>
    <Parameters>
      <testData>
        <testPriority>0</testPriority>
        <testDescription>Give values for the first time in attribute Dec-MMM and we can track it</testDescription>
      </testData>
      <teexmaData>
        <customerResPath>customerResPath</customerResPath>
        <teexmaVersion>teexmaVersion</teexmaVersion>
        <databasePath>databasePath</databasePath>
      </teexmaData>
    </Parameters>
  </TestHeader>
  <TxInterface>
    <Actions>
      <Login>
        <Parameters>
          <login>Administrator</login>
          <password>aaaaa</password>
        </Parameters>
      </Login>
      <MainWindow>
        <Actions>
          <NavigationTree>
            <Actions>
              <SelectOT>
                <Actions>
                  <Select>
                    <Parameters>
                      <value>TEST_Tracking</value>
                    </Parameters>
                  </Select>
                </Actions>
              </SelectOT>
              <EntityTree>
                <Actions>
                  <SelectEntity>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                  </SelectEntity>
                </Actions>
              </EntityTree>
            </Actions>
          </NavigationTree>
          <Edit>
            <Actions>
              <Content>
                <Actions>
                  <Write>
                    <DTDecimalValue>
                      <Parameters>
                        <id>165</id>
                        <value>
                          <min>10</min>
                          <max>12</max>
                          <mean>11</mean>
                        </value>
                      </Parameters>
                    </DTDecimalValue>
                  </Write>
                </Actions>
              </Content>
              <Ok>
                <Parameters>
                  <validate>true</validate>
                </Parameters>
              </Ok>
            </Actions>
          </Edit>
          <NavigationTree>
            <Actions>
              <TreeContextMenu>
                <Actions>
                  <ModificationsHistory>
                    <Parameters>
                      <nodes>
                        <path>New</path>
                      </nodes>
                    </Parameters>
                    <Actions>
                      <Content>
                        <Actions>
                          <ResultsList>
                            <Actions>
                              <ReadEntityRow>
                                <Parameters>
                                  <nodes>
                                    <path>Administrator</path>
                                  </nodes>
                                  <ExpectedValue>
                                    <collection>Administrator</collection>
                                  </ExpectedValue>
                                </Parameters>
                              </ReadEntityRow>
                              <ReadEntityRow>
                                <Parameters>
                                  <nodes>
                                    <path>Dec-MMM</path>
                                  </nodes>
                                  <ExpectedValue>
                                    <collection>from 10 to 12 - Mean value: 11</collection>
                                  </ExpectedValue>
                                </Parameters>
                              </ReadEntityRow>
                            </Actions>
                          </ResultsList>
                        </Actions>
                      </Content>
                    </Actions>
                  </ModificationsHistory>
                </Actions>
              </TreeContextMenu>
            </Actions>
          </NavigationTree>
        </Actions>
      </MainWindow>
    </Actions>
  </TxInterface>
</Actions>