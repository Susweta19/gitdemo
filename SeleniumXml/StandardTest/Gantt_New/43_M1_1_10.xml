<?xml version="1.0" encoding="utf-8"?>
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Select the option ""Days/Hours"" 
Select the option "None" from the button "Paging Scale"</testDescription>


			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Alice&#160;Dupont</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>


					<ClickOnButton>
						<Parameters>
							<buttonName>Gantt</buttonName>

						</Parameters>
					</ClickOnButton>
					<ClickOnButton>
						<Parameters>
							<buttonName>Multiressources view</buttonName>

						</Parameters>
					</ClickOnButton>
					<Sleep>
						<Parameters>
							<timeOut>6</timeOut>
						</Parameters>
					</Sleep>
					<ReturnGanttInNewTab>
						<Actions>
							<Changetab>
								<Parameters>
									<indexTab>1</indexTab>
								</Parameters>
							</Changetab>
							<Content>
								<Actions>
									<SelectScale>
										<Parameters>
											<option>Days/Hours</option>
										</Parameters>
									</SelectScale>
									<SelectPaging>
										<Parameters>
											<option>None</option>
										</Parameters>
									</SelectPaging>
									<ManageGanttView>
										<Actions>
											<ManageCommonMethods>
												<Actions>
													<GetNextDate>
														<Parameters>
															<days>0</days>
															<format>dd MMMM yyyy</format>
														</Parameters>
														<Hashtag>#FullFormatDate#</Hashtag>
													</GetNextDate>
												</Actions>
											</ManageCommonMethods>
											<ReadGanttFirstHeader>
												<Actions>
													<ElementAt>
														<Parameters>
															<index>1</index>
														</Parameters>
														<ExpectedResult>#FullFormatDate#</ExpectedResult>
													</ElementAt>
												</Actions>
											</ReadGanttFirstHeader>
											<ManageCommonMethods>
												<Actions>
													<GetNextDate>
														<Parameters>
															<days>30</days>
															<format>dd MMMM yyyy</format>
														</Parameters>
														<Hashtag>#dateafter30days#</Hashtag>
													</GetNextDate>
												</Actions>
											</ManageCommonMethods>
										</Actions>
									</ManageGanttView>
									<HorizontalScroll>
										<Parameters>
											<position>300000</position>
										</Parameters>
									</HorizontalScroll>
									<ManageGanttView>
										<Actions>
											<ReadGanttFirstHeader>
												<Actions>
													<LastElement>
														<ExpectedResult>#dateafter30days#</ExpectedResult>
													</LastElement>
												</Actions>
											</ReadGanttFirstHeader>
										</Actions>
									</ManageGanttView>
								</Actions>
							</Content>
						</Actions>
					</ReturnGanttInNewTab>


				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>