<?xml version="1.0" encoding="utf-8"?>
<Actions
	xsi:noNamespaceSchemaLocation="../../../ConsoleLauncher/bin/Debug/netcoreapp3.0/TxWebTests.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<TestHeader>
		<Parameters>
			<testData>
				<testPriority>0</testPriority>
				<testDescription>Slide the view into the future using the horizontal scrollbar and click "Now" from toolbar</testDescription>


			</testData>
			<teexmaData>
				<customerResPath>customerResPath</customerResPath>
				<teexmaVersion>teexmaVersion</teexmaVersion>
				<databasePath>databasePath</databasePath>
			</teexmaData>
		</Parameters>
	</TestHeader>
	<TxInterface>
		<Actions>
			<Login>
				<Parameters>
					<login>Alice&#160;Dupont</login>
					<password>aaaaa</password>
				</Parameters>
			</Login>
			<MainWindow>
				<Actions>


					<ClickOnButton>
						<Parameters>
							<buttonName>Gantt</buttonName>

						</Parameters>
					</ClickOnButton>
					<ClickOnButton>
						<Parameters>
							<buttonName>Projects/Tasks view</buttonName>

						</Parameters>
					</ClickOnButton>
					<ReturnGanttInNewTab>
						<Actions>
							<Changetab>
								<Parameters>
									<indexTab>1</indexTab>
								</Parameters>
							</Changetab>
							<Content>
								<Actions>
									<SelectScale>
										<Parameters>
											<option>Days/Hours</option>
										</Parameters>
									</SelectScale>
									<SelectPaging>
										<Parameters>
											<option>None</option>
										</Parameters>
									</SelectPaging>
									<ManageTask>
										<Parameters></Parameters>
										<Actions></Actions>
									</ManageTask>
									<ManageGanttView>
										<Actions>
											<ReadGanttFirstHeader>
												<Actions>
													<ElementAt>
														<Parameters>
															<index>0</index>
														</Parameters>
														<Hashtag>#day1#</Hashtag>
													</ElementAt>
												</Actions>
											</ReadGanttFirstHeader>
											<ReadGanttFirstHeader>
												<Actions>
													<ElementAt>
														<Parameters>
															<index>1</index>
														</Parameters>
														<Hashtag>#day2#</Hashtag>
													</ElementAt>
												</Actions>
											</ReadGanttFirstHeader>
										</Actions>
									</ManageGanttView>
									<HorizontalScroll>
										<Parameters>
											<position>300000</position>
										</Parameters>
									</HorizontalScroll>
									<ManageGanttView>
										<Actions>
											<ReadGanttFirstHeader>
												<Actions>
													<LastElement>
														<Hashtag>#lastday#</Hashtag>
													</LastElement>
												</Actions>
											</ReadGanttFirstHeader>
										</Actions>
									</ManageGanttView>
									<Sleep>
										<Parameters>
											<timeOut>2</timeOut>
										</Parameters>
									</Sleep>
									<ClickNow></ClickNow>
									<Sleep>
										<Parameters>
											<timeOut>2</timeOut>
										</Parameters>
									</Sleep>
									<ManageGanttView>
										<Actions>
											<ReadGanttFirstHeader>
												<Actions>
													<ElementAt>
														<Parameters>
															<index>0</index>
														</Parameters>
														<ExpectedResult>#day1#</ExpectedResult>
													</ElementAt>
													<ElementAt>
														<Parameters>
															<index>1</index>
														</Parameters>
														<ExpectedResult>#day2#</ExpectedResult>
													</ElementAt>
												</Actions>
											</ReadGanttFirstHeader>
										</Actions>
									</ManageGanttView>
								</Actions>
							</Content>
						</Actions>
					</ReturnGanttInNewTab>


				</Actions>
			</MainWindow>
		</Actions>
	</TxInterface>
</Actions>